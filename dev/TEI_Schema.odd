               <remarks xml:lang="de" versionDate="2025-02-06">
                  <p>In den Kindelementen werden alle Metadaten zu einem Textträger, wie Aufbewahrungsort,
      Material, Layout, Schreiber, Siegel und Bibliographie erfasst.</p>
               </remarks>
               <listRef>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/MS.html#msov">11.1. Overview</ref>
               </listRef>
            </elementSpec>
            <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                         xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                         rend="change"
                         module="msdescription"
                         xml:id="DIMENSIONS"
                         ident="dimensions">
               <gloss rend="replace" xml:lang="de" versionDate="2025-02-03">Dimensionen</gloss>
               <gloss rend="replace" xml:lang="en" versionDate="2025-02-03">dimensions</gloss>
               <gloss rend="replace" xml:lang="fr" versionDate="2025-02-03">dimensions</gloss>
               <desc rend="replace" xml:lang="de" versionDate="2025-02-06">Enthält die Masse einer Quelle.</desc>
               <desc rend="replace" xml:lang="en" versionDate="2023-04-19">Dimension of a source</desc>
               <desc rend="replace" xml:lang="fr" versionDate="2023-04-19">Dimensions d'une source</desc>
               <classes>
                  <memberOf key="att.typed"/>
                  <memberOf key="model.pPart.msdesc"/>
               </classes>
               <content>
                  <sequence minOccurs="1" maxOccurs="1" preserveOrder="false">
                     <elementRef key="width"/>
                     <elementRef key="height" minOccurs="0"/>
                  </sequence>
               </content>
               <constraintSpec xml:lang="en" scheme="schematron" ident="sch-el-dimensions">
                  <desc xml:lang="en" versionDate="2023-04-19">constraint for tei:dimensions</desc>
                  <constraint>
                     <sch:pattern>
                        <sch:rule context="tei:dimensions[@type = 'plica']">
                           <sch:assert test="count(*) = 1 and */name() = 'width'">If the value of @type is
            'plica' only one tei:width is allowed as a child.</sch:assert>
                        </sch:rule>
                     </sch:pattern>
                  </constraint>
               </constraintSpec>
               <constraintSpec scheme="schematron" ident="duplicateDim">
    <!-- It would be simpler to use just
         "( count(tei:width), count(tei:height), count(tei:depth) ) > 1",
         but then we would not be able to tell the user which element
         had too many occurences. -->
                  <constraint>
                     <sch:rule context="tei:dimensions">
                        <sch:report test="count(tei:width) gt 1">
          The element <sch:name/> may appear once only
        </sch:report>
                        <sch:report test="count(tei:height) gt 1">
          The element <sch:name/> may appear once only
        </sch:report>
                        <sch:report test="count(tei:depth) gt 1">
          The element <sch:name/> may appear once only
        </sch:report>
                     </sch:rule>
                  </constraint>
               </constraintSpec>
               <attList>
                  <attDef ident="type" mode="replace" usage="req">
                     <desc xml:lang="de" versionDate="2025-01-31">die Art des Masses</desc>
                     <desc xml:lang="en" versionDate="2025-01-31">the type of dimension</desc>
                     <desc xml:lang="fr" versionDate="2025-01-31">le type de dimension</desc>
                     <datatype>
                        <dataRef key="ssrq.type.dimensions"/>
                     </datatype>
                  </attDef>
               </attList>
               <exemplum xml:lang="de" xml:id="ex-extent-de">
                  <p>Beispiel für die Massangabe einer Quelle</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples">
                     <extent>
                        <dimensions type="leaves">
                           <height unit="cm" quantity="33.0"/>
                           <width unit="cm" quantity="24.0"/>
                        </dimensions>
                        <dimensions type="plica">
                           <width unit="cm" quantity="4.0"/>
                        </dimensions>
                     </extent>
                  </egXML>
               </exemplum>
               <exemplum xml:lang="en" xml:id="ex-extent-en">
                  <p>Example of the dimension of a source</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples">
                     <extent>
                        <dimensions type="leaves">
                           <height unit="cm" quantity="33.0"/>
                           <width unit="cm" quantity="24.0"/>
                        </dimensions>
                        <dimensions type="plica">
                           <width unit="cm" quantity="4.0"/>
                        </dimensions>
                     </extent>
                  </egXML>
               </exemplum>
               <exemplum xml:lang="fr" xml:id="ex-extent-fr">
                  <p>Exemple de mesure d'une source</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples">
                     <extent>
                        <dimensions type="leaves">
                           <height unit="cm" quantity="33.0"/>
                           <width unit="cm" quantity="24.0"/>
