               <gloss rend="replace" xml:lang="de" versionDate="2025-02-04">Siegel</gloss>
               <gloss rend="replace" xml:lang="en" versionDate="2025-02-04">seal</gloss>
               <gloss rend="replace" xml:lang="fr" versionDate="2025-02-04">sceau</gloss>
               <desc rend="replace" xml:lang="de" versionDate="2023-07-15">Beschreibt ein Siegel.</desc>
               <desc rend="replace" xml:lang="en" versionDate="2023-07-15">Describes a seal.</desc>
               <desc rend="replace" xml:lang="fr" versionDate="2023-07-15">Décrit un sceau.</desc>
               <classes>
