               <gloss versionDate="2008-03-30" xml:lang="fr">partie de texte endommagée</gloss>
               <desc rend="replace" xml:lang="de" versionDate="2023-05-11">Bezeichnet grössere Abschnitte, die beschädigt sind.</desc>
               <desc rend="replace" xml:lang="en" versionDate="2023-05-11">Denotes larger sections that are damaged.</desc>
               <desc rend="replace" xml:lang="fr" versionDate="2023-05-11">Désigne des sections plus grandes qui sont endommagées.</desc>
               <classes>
                  <memberOf key="att.damaged"/>
                  <memberOf key="att.spanning"/>
               </classes>
               <content>
                  <empty/>
               </content>
