            <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                       xmlns:xi="http://www.w3.org/2001/XInclude"
                       rend="change"
                       predeclare="true"
                       module="transcr"
                       type="atts"
                       ident="att.global.facs">
               <desc versionDate="2021-05-09" xml:lang="en">provides attributes used to express correspondence between an element and all or part of a facsimile image or surface.</desc>
               <desc versionDate="2009-11-16" xml:lang="fr">attributs utilisables pour les éléments correspondant à tout ou partie d'une image, parce qu'ils contiennent une représentation alternative de cette image, généralement mais
    pas nécessairement, une transcription.</desc>
               <classes/>
               <attList>
                  <attDef ident="facs" mode="replace">
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="de"
                           versionDate="2023-05-17">Ordnet eine Textseite einem
                        Faksimile oder einem faksimilierten Ausschnitt zu.
                        Erlaubt sind beliebig viele Gross- und Kleinbuchstaben
                        von A-Z, Unterstriche, Bindestriche, die Ziffern 0-9,
                        gefolgt von "r", "v" oder "plica".
                    </desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="en"
                           versionDate="2023-04-06">Assigns a facsimile or facsimile
                        extract to a page of text. Any number of upper and
                        lower case letters from A to Z, underscores, hyphens,
                        the numbers 0 to 9, followed by "r", "v" or "plica" are
                        permitted.
                    </desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="fr"
                           versionDate="2023-04-06">Associe une page de texte à un
                        fac-similé ou à un extrait de fac-similé. N'importe
                        quel nombre de lettres majuscules et minuscules de A à Z,
                        les traits de soulignement, les traits d'union, les
                        chiffres de 0 à 9, suivis de "r", "v" ou "plica" sont
                        autorisés.
                    </desc>
                     <datatype xmlns:sch="http://purl.oclc.org/dsdl/schematron">
                        <rng:list>
                           <rng:oneOrMore>
                              <ref xmlns="http://relaxng.org/ns/structure/1.0" name="ssrq.facs.name"/>
                           </rng:oneOrMore>
                        </rng:list>
                     </datatype>
                  </attDef>
               </attList>
               <listRef>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/PH.html#PHFAX">11.1. Digital Facsimiles</ref>
               </listRef>
            </classSpec>
            <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                         xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                         xmlns:xi="http://www.w3.org/2001/XInclude"
                         rend="change"
                         module="transcr"
                         ident="addSpan">
               <gloss versionDate="2005-01-14" xml:lang="en">added span of text</gloss>
               <gloss versionDate="2007-06-12" xml:lang="fr">partie de texte ajoutée</gloss>
               <desc rend="replace" xml:lang="de" versionDate="2023-08-18">Bezeichnet grössere Hinzufügungen.</desc>
               <desc rend="replace" xml:lang="en" versionDate="2023-07-13">Denotes major additions.</desc>
               <desc rend="replace" xml:lang="fr" versionDate="2023-07-13">Désigne des ajouts majeurs.</desc>
               <classes>
                  <memberOf key="att.global.rendition"/>
                  <memberOf key="att.placement"/>
                  <memberOf key="att.spanning"/>
                  <memberOf key="att.typed"/>
                  <memberOf key="att.written"/>
               </classes>
               <content>
                  <empty/>
               </content>
               <constraintSpec ident="addSpan-requires-spanTo" scheme="schematron">
                  <constraint>
                     <sch:assert test="@spanTo">The @spanTo attribute of <sch:name/> is required.</sch:assert>
                  </constraint>
               </constraintSpec>
               <constraintSpec ident="addSpan-requires-spanTo-fr"
                               scheme="schematron"
                               xml:lang="fr">
                  <constraint>
                     <sch:assert test="@spanTo">L'attribut spanTo est requis.</sch:assert>
                  </constraint>
               </constraintSpec>
               <attList>
                  <attDef ident="place" mode="replace">
                     <datatype>
                        <rng:choice>
                           <ref xmlns="http://relaxng.org/ns/structure/1.0" name="ssrq.place"/>
                           <ref xmlns="http://relaxng.org/ns/structure/1.0" name="ssrq.place.add"/>
                        </rng:choice>
                     </datatype>
                  </attDef>
                  <attDef ident="rend" mode="replace">
                     <datatype>
                        <dataRef key="ssrq.rend.add"/>
                     </datatype>
                  </attDef>
                  <attDef ident="type" mode="replace">
                     <datatype>
                        <dataRef key="ssrq.type.add"/>
                     </datatype>
                  </attDef>
               </attList>
               <exemplum xml:lang="de" xml:id="ex-addSpan-de">
                  <p>Beispiel für eine umfangreichere Hinzufügung</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples">
                     <addSpan spanTo="add1" hand="otherHand" place="bottom" rend="pencil"/>
                     <lb/>Presentibus
    <lb/>Antonio nepote domini officialis, fratre Germano ordinis Predicatorum; dominus
    <lb/>officialis prefatus monuit dictam Jordanam pro secunda dilatione
    <lb/>et assignata est ad cras pro tertia.
    <lb/>Delayens
    <anchor xml:id="add1"/>
                  </egXML>
               </exemplum>
               <exemplum xml:lang="en" xml:id="ex-addSpan-en">
                  <p>Example of a larger addition</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples">
                     <addSpan spanTo="add1" hand="otherHand" place="bottom" rend="pencil"/>
                     <lb/>Presentibus
    <lb/>Antonio nepote domini officialis, fratre Germano ordinis Predicatorum; dominus
    <lb/>officialis prefatus monuit dictam Jordanam pro secunda dilatione
    <lb/>et assignata est ad cras pro tertia.
    <lb/>Delayens
    <anchor xml:id="add1"/>
                  </egXML>
               </exemplum>
               <exemplum xml:lang="fr" xml:id="ex-addSpan-fr">
                  <p>Exemple d'un ajout plus important</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples">
                     <addSpan spanTo="add1" hand="otherHand" place="bottom" rend="pencil"/>
                     <lb/>Presentibus
    <lb/>Antonio nepote domini officialis, fratre Germano ordinis Predicatorum; dominus
    <lb/>officialis prefatus monuit dictam Jordanam pro secunda dilatione
    <lb/>et assignata est ad cras pro tertia.
    <lb/>Delayens
    <anchor xml:id="add1"/>
                  </egXML>
               </exemplum>
               <remarks xml:lang="de" versionDate="2023-07-11">
                  <p>Mit <att>spanTo</att> wird zwingend auf den Endpunkt der Hinzufügung verwiesen, der
            mit <gi>anchor</gi> markiert wird.
        </p>
                  <p>Der Ort der Hinzufügung muss zwingend in <att>place</att> und die Hand der ergänzten Stelle kann
            mit <att>hand</att> festgehalten werden. Falls notwendig, kann innerhalb von <att>rend</att> angegeben
            werden, wie die Hinzufügung (mit Bleistift, anderer Tinte etc.) vorgenommen wurde. Handelt es sich bei einer
            Ergänzung um die Hand des ersten Schreibers, dann wird die Hand nicht speziell angemerkt.
        </p>
               </remarks>
               <remarks xml:lang="en" versionDate="2023-07-11">
                  <p>
                     <att>spanTo</att>
            is used to refer to the end point of the addition which is marked with <gi>anchor</gi>.
        </p>
                  <p>The place of addition must be in <att>place</att> and the hand of the added place can be held
            with <att>hand</att>. If necessary, inside <att>rend</att> it can be specified how the addition was made
            (pencil, other ink, etc.). If an addition is the hand of the first scribe, then the hand is not specifically
            noted.
        </p>
               </remarks>
               <remarks xml:lang="fr" versionDate="2023-07-11">
                  <p>
                     <att>spanTo</att>
            est utilisé pour faire référence au point final de l'addition, qui est marquée par <gi>anchor</gi>.
        </p>
                  <p>La place d'addition doit être dans <att>place</att> et la main de la place ajoutée peut être tenue avec <att>
            hand</att>. Si nécessaire, à l'intérieur de <att>rend</att>, il est possible de spécifier comment l'ajout a
            été effectué (crayon, autre encre, etc.). Si un ajout est la main du premier scribe, alors la main n'est pas
            spécifiquement notée.
        </p>
               </remarks>
               <listRef>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/PH.html#PHAD">11.3.1.4. Additions and Deletions</ref>
               </listRef>
            </elementSpec>
            <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                         xmlns:xi="http://www.w3.org/2001/XInclude"
                         rend="change"
                         module="transcr"
                         ident="damage">
               <gloss xml:lang="en" versionDate="2007-06-12">damage</gloss>
               <gloss versionDate="2007-06-12" xml:lang="fr">dommage</gloss>
               <desc rend="replace" xml:lang="de" versionDate="2023-07-13">Bezeichnet einen beschädigten Abschnitt.</desc>
               <desc rend="replace" xml:lang="en" versionDate="2023-07-13">Denotes a damaged section.</desc>
               <desc rend="replace" xml:lang="fr" versionDate="2023-07-13">Indique une section endommagée.</desc>
               <classes>
                  <memberOf key="att.damaged"/>
                  <memberOf key="att.dimensions"/>
               </classes>
               <content>
                  <alternate minOccurs="1" maxOccurs="1">
                     <alternate minOccurs="1" maxOccurs="1">
                        <elementRef key="add"/>
                        <elementRef key="gap"/>
                        <elementRef key="supplied"/>
                        <elementRef key="unclear"/>
                     </alternate>
                     <alternate minOccurs="1" maxOccurs="1">
                        <sequence minOccurs="1" maxOccurs="1" preserveOrder="false">
                           <elementRef key="gap"/>
                           <elementRef key="supplied"/>
                        </sequence>
                        <sequence minOccurs="1" maxOccurs="1" preserveOrder="false">
                           <elementRef key="gap"/>
                           <elementRef key="unclear"/>
                        </sequence>
                        <sequence minOccurs="1" maxOccurs="1" preserveOrder="false">
                           <elementRef key="supplied"/>
                           <elementRef key="unclear"/>
                        </sequence>
                     </alternate>
                  </alternate>
               </content>
               <attList>
                  <attDef ident="unit" mode="replace">
                     <datatype>
                        <rng:choice>
                           <ref xmlns="http://relaxng.org/ns/structure/1.0"
                                name="ssrq.measure.lengths.base.cm"/>
                           <ref xmlns="http://relaxng.org/ns/structure/1.0"
                                name="ssrq.measure.lengths.base.line"/>
                           <ref xmlns="http://relaxng.org/ns/structure/1.0" name="ssrq.measure.typography"/>
                        </rng:choice>
                     </datatype>
                  </attDef>
                  <attDef ident="quantity" mode="change">
                     <datatype>
                        <rng:choice>
                           <rng:data type="float">
                              <rng:param name="minInclusive">0</rng:param>
                           </rng:data>
                           <rng:data type="positiveInteger"/>
                        </rng:choice>
                     </datatype>
                  </attDef>
               </attList>
               <exemplum xml:lang="de" xml:id="ex-damage-add-de">
                  <p>Beispiel für eine beschädigte Stelle, in die eine andere Hand Text eingetragen hat</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples">
                     <damage agent="water">
                        <add hand="otherHand" place="overwritten">zuͦ trincken</add>
                     </damage>
                  </egXML>
               </exemplum>
               <exemplum xml:lang="en" xml:id="ex-damage-add-en">
                  <p>Example of a damaged area where text has been written by another hand</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples">
                     <damage agent="water">
                        <add hand="otherHand" place="overwritten">zuͦ trincken</add>
                     </damage>
                  </egXML>
               </exemplum>
               <exemplum xml:lang="fr" xml:id="ex-damage-add-fr">
                  <p>Exemple d'une zone endommagée où le texte a été écrit par une autre main</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples">
                     <damage agent="water">
                        <add hand="otherHand" place="overwritten">zuͦ trincken</add>
                     </damage>
                  </egXML>
               </exemplum>
               <exemplum xml:lang="de" xml:id="ex-damage-gap-de">
