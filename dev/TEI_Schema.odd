                  <egXML xmlns="http://www.tei-c.org/ns/Examples">
        das best hoͧbt aͧn ein hoͧbt oder <del rend="strikethrough">das</del>
                     <lb/>das best gewant oder
    </egXML>
               </exemplum>
               <exemplum xml:lang="en" xml:id="ex-del-en">
