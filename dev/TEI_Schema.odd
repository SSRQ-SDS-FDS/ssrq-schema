            <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                         rend="change"
                         module="figures"
                         ident="table">
               <gloss xml:lang="en" versionDate="2007-06-12">table</gloss>
               <gloss versionDate="2007-06-12" xml:lang="fr">tableau</gloss>
               <gloss versionDate="2017-06-19" xml:lang="de">Tabelle</gloss>
               <desc xmlns:xi="http://www.w3.org/2001/XInclude"
                     rend="replace"
                     xml:lang="de"
                     versionDate="2023-07-11">Beschreibt eine Tabelle.</desc>
               <desc xmlns:xi="http://www.w3.org/2001/XInclude"
                     rend="replace"
                     xml:lang="en"
                     versionDate="2023-07-11">Describes a table.</desc>
               <desc xmlns:xi="http://www.w3.org/2001/XInclude"
                     rend="replace"
                     xml:lang="fr"
                     versionDate="2023-07-11">Décrit un tableau.</desc>
               <classes/>
               <content>
                  <sequence minOccurs="1" maxOccurs="1" preserveOrder="false">
                     <alternate minOccurs="0" maxOccurs="unbounded">
                        <elementRef xmlns:xi="http://www.w3.org/2001/XInclude" key="cb"/>
                        <elementRef xmlns:xi="http://www.w3.org/2001/XInclude" key="head"/>
                        <elementRef xmlns:xi="http://www.w3.org/2001/XInclude" key="pb"/>
                     </alternate>
                     <elementRef xmlns:xi="http://www.w3.org/2001/XInclude"
                                 key="row"
                                 maxOccurs="unbounded"/>
                  </sequence>
               </content>
               <attList>
                  <attDef xmlns:xi="http://www.w3.org/2001/XInclude" ident="rows" mode="delete"/>
                  <attDef xmlns:xi="http://www.w3.org/2001/XInclude" ident="cols" mode="delete"/>
               </attList>
               <exemplum xml:lang="de" xml:id="ex-table-de">
                  <p xmlns:xi="http://www.w3.org/2001/XInclude">Beispiel für eine Preisliste für Wein mit vier Spalten, wovon die letzten drei Spalten mit «gbz», «xr»
                bzw. «dn» überschrieben sind
            </p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples"
                         xmlns:xi="http://www.w3.org/2001/XInclude">
                     <table>
                        <head>Von dem wein</head>
                        <row>
                           <cell/>
                           <cell role="label">gbz</cell>
                           <cell role="label">xr</cell>
                           <cell role="label">₰</cell>
                        </row>
                        <row>
                           <cell>für j saum wein</cell>
                           <cell>–</cell>
                           <cell>1</cell>
                           <cell>–</cell>
                        </row>
                        <pb/>
                        <head>item</head>
                        <row>
                           <cell>für j lägelen wein</cell>
                           <cell>–</cell>
                           <cell>–</cell>
                           <cell>2</cell>
                        </row>
