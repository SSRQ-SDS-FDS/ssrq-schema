                      module="derived-module-ssrq-main-schema"
                      ident="ssrq.type.note"
                      mode="add">
               <content xmlns:rng="http://relaxng.org/ns/structure/1.0">
                  <valList type="closed">
                     <valItem ident="text_comparison">
                        <desc xml:lang="de" versionDate="2023-10-30">Verweis auf parallele Textstelle</desc>
                        <desc xml:lang="en" versionDate="2023-10-30">reference to parallel passage</desc>
                        <desc xml:lang="fr" versionDate="2023-10-30">référence à un passage parallèle</desc>
                        <desc xml:lang="it" versionDate="2024-05-08">riferimento ad un passaggio parallelo</desc>
                     </valItem>
                  </valList>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      module="derived-module-ssrq-main-schema"
                      ident="ssrq.type.pb"
                      mode="add">
               <content xmlns:rng="http://relaxng.org/ns/structure/1.0">
                  <valList type="closed">
                     <valItem ident="original">
                        <desc xml:lang="de" versionDate="2023-04-06">Originalseitenwechsel</desc>
                        <desc xml:lang="en" versionDate="2025-02-03">original pagination</desc>
                        <desc xml:lang="fr" versionDate="2025-02-03">saut de page original</desc>
                        <desc xml:lang="it" versionDate="2025-02-03">impaginazione originale</desc>
                     </valItem>
                  </valList>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      module="derived-module-ssrq-main-schema"
                      ident="ssrq.type.quote"
                      mode="add">
               <content xmlns:rng="http://relaxng.org/ns/structure/1.0">
                  <valList type="closed">
                     <valItem ident="fully_inserted">
                        <desc xml:lang="de" versionDate="2025-02-03">ein vollständiges Insert einer anderen Urkunde</desc>
                        <desc xml:lang="en" versionDate="2025-02-03">a complete insert of another document</desc>
                        <desc xml:lang="fr" versionDate="2025-02-03">un insert complet d’un autre document</desc>
                        <desc xml:lang="it" versionDate="2025-02-03">un inserto completo di un altro documento</desc>
                     </valItem>
                     <valItem ident="partially_inserted">
                        <desc xml:lang="de" versionDate="2025-02-03">ein unvollständiges Insert einer anderen Urkunde</desc>
                        <desc xml:lang="en" versionDate="2025-02-03">an incomplete insert of another document</desc>
                        <desc xml:lang="fr" versionDate="2025-02-03">un insert incomplet d’un autre document</desc>
                        <desc xml:lang="it" versionDate="2025-02-03">un inserto incompleto di un altro documento</desc>
                     </valItem>
                  </valList>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      module="derived-module-ssrq-main-schema"
                      ident="ssrq.type.subst"
                      mode="add">
               <content xmlns:rng="http://relaxng.org/ns/structure/1.0">
                  <valList type="closed">
                     <valItem ident="cypher">
                        <desc xml:lang="de" versionDate="2025-02-14">die Ersetzung einer Chiffre</desc>
                        <desc xml:lang="en" versionDate="2025-02-14">the substitution of a cypher</desc>
                        <desc xml:lang="fr" versionDate="2025-02-14">la substitution d'un chiffre</desc>
                     </valItem>
                  </valList>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      module="derived-module-ssrq-main-schema"
                      ident="ssrq.type.term"
                      mode="add">
               <content xmlns:rng="http://relaxng.org/ns/structure/1.0">
                  <valList type="closed">
                     <valItem ident="index">
                        <desc xml:lang="de" versionDate="2025-02-03">Stichwort für den Index</desc>
                        <desc xml:lang="en" versionDate="2025-02-03">keyword for the index</desc>
                        <desc xml:lang="fr" versionDate="2025-02-03">mot-clé pour l'index</desc>
                     </valItem>
                     <valItem ident="unknown">
                        <desc xml:lang="de" versionDate="2025-02-03">unbekannter Ausdruck oder unklare Bedeutung</desc>
                        <desc xml:lang="en" versionDate="2025-02-03">unknown expression or unclear meaning</desc>
                        <desc xml:lang="fr" versionDate="2025-02-03">expression inconnue ou signification peu claire</desc>
                     </valItem>
                  </valList>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      module="derived-module-ssrq-main-schema"
                      ident="ssrq.type.text"
                      mode="add">
               <content xmlns:rng="http://relaxng.org/ns/structure/1.0">
                  <valList type="closed">
                     <valItem ident="collection">
                        <desc xml:lang="de" versionDate="2023-09-12">Sammlung von Stücken</desc>
                        <desc xml:lang="en" versionDate="2025-02-03">collection of source documents</desc>
                        <desc xml:lang="fr" versionDate="2025-02-03">collection de documents sources</desc>
                        <desc xml:lang="it" versionDate="2025-02-03">raccolta dei documenti originali</desc>
                     </valItem>
                     <valItem ident="summary">
                        <desc xml:lang="de" versionDate="2023-04-05">Regest des Quellstücks</desc>
                        <desc xml:lang="en" versionDate="2025-02-03">regest of the source document</desc>
                        <desc xml:lang="fr" versionDate="2025-02-03">enregistrement du document source</desc>
                        <desc xml:lang="it" versionDate="2025-02-03">regest del documento di origine</desc>
                     </valItem>
                     <valItem ident="transcript">
                        <desc xml:lang="de" versionDate="2023-04-06">Transkript des Quellstücks</desc>
                        <desc xml:lang="en" versionDate="2025-02-03">transcript of the source document</desc>
                        <desc xml:lang="fr" versionDate="2025-02-03">transcription du document source</desc>
                        <desc xml:lang="it" versionDate="2025-02-03">trascrizione del documento di origine</desc>
                     </valItem>
                  </valList>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      module="derived-module-ssrq-main-schema"
                      ident="ssrq.xmlid.anchor"
                      mode="add">
               <content xmlns:rng="http://relaxng.org/ns/structure/1.0">
                  <dataRef name="ID" restriction="(del|damage|add)[1-9][0-9]*"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.date.point"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für Datumsangaben</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for dates</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour les dates</desc>
               <content>
                  <dataRef name="string"
                           restriction="(((\d{4}|-)-(0[1-9]|1[0-2]))|--)-(0[1-9]|[1-2][0-9]|3[0-1])"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.time.point"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für Zeitangaben</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for times</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour les temps</desc>
               <content>
                  <dataRef name="string"
                           restriction="(((2[0-3]|[0-1][0-9]):[0-5][0-9]:[0-5][0-9])|24:00:00)"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.time.duration"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für Zeitspannen nach ISO 8601</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for time spans according to ISO 8601</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour le temps s'étend selon ISO 8601</desc>
               <content>
                  <dataRef name="string"
                           restriction="(R/)?PT(([0-1][0-9]|2[0-4])H)?([0-5][0-9]M)?([0-5][0-9](\.[0-9]+)?S)?"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.foliation"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für die Blattzählung</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for the foliation</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour le foliation</desc>
               <content>
                  <dataRef name="string"
                           restriction="((([1-9]\d*)|([IVXLCDM]+))([ab]|bis)?[rv])|[rv]"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pagination"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für die Seitenzählung</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for the pagination</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour le pagination</desc>
               <content>
                  <dataRef name="string"
                           restriction="(([1-9]\d*(\.[1-9]\d*)?|([IVXLCDM]+))([ab]|bis)?)|cover"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.section"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für die Abschnittszählung</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for the section numbering</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour la numérotation des sections</desc>
               <content>
                  <dataRef name="string" restriction="s([1-9]\d*)"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.numbering"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-02-03">die originale Paginierung, Foliierung oder Zählung der Pergamentstücke</desc>
               <desc xml:lang="en" versionDate="2025-02-03">the original pagination, foliation or counting the pieces of parchment</desc>
               <desc xml:lang="fr" versionDate="2025-02-03">la pagination originale, la foliation ou la numérotation des morceaux de parchemin</desc>
               <content>
                  <alternate minOccurs="1" maxOccurs="1">
                     <dataRef key="ssrq.foliation"/>
                     <dataRef key="ssrq.pagination"/>
                     <dataRef key="ssrq.section"/>
                  </alternate>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.facs.name"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für Faksimilenamen</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for facsimile names</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle poul les noms de télécopie</desc>
               <content>
                  <dataRef name="string" restriction="[A-Za-z_\-\d]+(\d|[IVXLCDM]|[rv]|plica)"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.keywords"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für Schlagwort-IDs</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for keyword IDs</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour les ID de mots-clés</desc>
               <content>
                  <dataRef name="string" restriction="key\d{6}"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.lemma"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für Lemma-IDs</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for lemma IDs</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour les ID de lemmes</desc>
               <content>
                  <dataRef name="string" restriction="lem\d{6}(\.1?\d{2})?"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.org"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für Organisations-IDs</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for organisation IDs</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour les ID de organisations</desc>
               <content>
                  <dataRef name="string" restriction="org\d{6}(\.\d{2})?"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.persons"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für Personen-IDs</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for person IDs</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour les ID de personne</desc>
               <content>
                  <dataRef name="string" restriction="per\d{6}[abc]?(\.\d{2})?"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.places"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für Ort-IDs</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for place IDs</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour les ID de lieux</desc>
               <content>
                  <dataRef name="string" restriction="loc\d{6}(\.\d{2})?"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.databases"
                      mode="add"
                      module="ssrq.core.module">
               <content>
                  <alternate minOccurs="1" maxOccurs="1">
                     <dataRef key="ssrq.pointer.keywords"/>
                     <dataRef key="ssrq.pointer.lemma"/>
                     <dataRef key="ssrq.pointer.org"/>
                     <dataRef key="ssrq.pointer.persons"/>
                     <dataRef key="ssrq.pointer.places"/>
                  </alternate>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.doi"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für eine DOI</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for a DOI.</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour une DOI.</desc>
               <content>
                  <dataRef name="string" restriction="doi:10\.\d{4}/.*"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.url"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für eine URL</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for a URL</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour une URL</desc>
               <content>
                  <dataRef name="anyURI" restriction="(https?|ftp)://[^\s/$.?#].[^\s]*"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.urn"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für eine URN</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for a URN</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour une URN</desc>
               <content>
                  <dataRef name="string"
                           restriction="urn:ssrq:(SSRQ|SDS|FDS)-([A-Z]{2})-([A-Za-z0-9_]+)-(((([A-Za-z0-9]+\.)*)([0-9]+)-(1))|(lit|intro|bailiffs))(#(\d+|fol\d+[rv](\-\d+[rv])?|[pn]\d+(\.\d+)?(\-\d+(\.\d+)?)?))?"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.target"
                      mode="add"
                      module="ssrq.core.module">
               <content>
                  <alternate minOccurs="1" maxOccurs="1">
                     <dataRef key="ssrq.pointer.doi"/>
                     <dataRef key="ssrq.pointer.url"/>
                     <dataRef key="ssrq.pointer.urn"/>
                  </alternate>
               </content>
            </dataSpec>
            <dataSpec rend="add" ident="ssrq.id" mode="add" module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für eine SSRQ UUID</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for an SLS UUID</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour un UUID SDS</desc>
               <content>
                  <dataRef name="ID"
                           restriction="id-ssrq-[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[4][0-9A-Fa-f]{3}-[89ABab][0-9A-Fa-f]{3}-[0-9A-Fa-f]{12}"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.id.string"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für eine SSRQ UUID</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for an SLS UUID</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour un UUID SDS</desc>
               <content>
                  <dataRef name="string"
                           restriction="id-ssrq-[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[4][0-9A-Fa-f]{3}-[89ABab][0-9A-Fa-f]{3}-[0-9A-Fa-f]{12}"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.id"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für einen Pointer mit Fragmentbezeichner auf SSRQ IDs</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for a pointer with fragment identifier on SLS IDS</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour un pointeur avec un identifiant de fragment sur les ID SDS</desc>
               <content>
                  <dataRef name="string"
                           restriction="id-ssrq-[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[4][0-9A-Fa-f]{3}-[89ABab][0-9A-Fa-f]{3}-[0-9A-Fa-f]{12}(#(\d+|fol\d+[rv](\-\d+[rv])?|[pn]\d+(\.\d+)?(\-\d+(\.\d+)?)?))?"/>
               </content>
            </dataSpec>
