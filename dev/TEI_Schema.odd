                     <num value="0.75">3/4</num>
                  </egXML>
               </exemplum>
               <exemplum xml:lang="fr" xml:id="ex-num-fr">
                  <p xmlns:xi="http://www.w3.org/2001/XInclude">Exemple de chiffres romains</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples"
                         xmlns:xi="http://www.w3.org/2001/XInclude">
