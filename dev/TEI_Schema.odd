               <attList>
                  <attDef ident="type" mode="delete"/>
                  <attDef ident="value" usage="req">
                     <desc rend="replace" xml:lang="de" versionDate="2025-02-03">der Zahlwert in arabischen Ziffern</desc>
                     <desc rend="replace" xml:lang="en" versionDate="2025-02-03">the numerical value in Arabic numerals</desc>
                     <desc rend="replace" xml:lang="fr" versionDate="2025-02-03">la valeur numérique en chiffres arabes</desc>
                     <datatype>
                        <rng:choice>
                           <rng:data type="float"/>
                           <rng:data type="int"/>
                        </rng:choice>
                     </datatype>
                     <valDesc>a numeric value.</valDesc>
                     <valDesc xml:lang="fr">une valeur numérique.</valDesc>
                     <valDesc xml:lang="de">ein numerischer Wert</valDesc>
                  </attDef>
               </attList>
               <exemplum xml:lang="de" xml:id="ex-hi-num-de">
                  <p>Erfassung einer römischen Ziffer</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples">
