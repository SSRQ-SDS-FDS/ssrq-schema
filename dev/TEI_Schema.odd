                     <elementRef xmlns:xi="http://www.w3.org/2001/XInclude" key="lb"/>
                     <elementRef xmlns:xi="http://www.w3.org/2001/XInclude" key="pb"/>
                  </alternate>
               </content>
               <attList>
                  <attDef xmlns:xi="http://www.w3.org/2001/XInclude" ident="type" mode="delete"/>
                  <attDef ident="value" usage="req">
                     <desc xmlns:xi="http://www.w3.org/2001/XInclude"
                           rend="replace"
