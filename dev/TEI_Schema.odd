   <text>
      <body>
         <schemaSpec xmlns:teix="http://www.tei-c.org/ns/Examples"
                     ident="ssrq-main-schema"
                     docLang="de en fr"
                     defaultExceptions="http://www.tei-c.org/ns/1.0 teix:egXML"
                     source="https://www.tei-c.org/Vault/P5/4.5.0/xml/tei/odd/p5subset.xml">
            <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                       xmlns:xi="http://www.w3.org/2001/XInclude"
                       rend="change"
                       module="tei"
                       type="atts"
                       ident="att.canonical">
               <desc versionDate="2008-09-05" xml:lang="en">provides attributes that can be used to associate a representation such as a name or title
    with canonical information about the object being named or referenced.</desc>
               <desc versionDate="2009-05-25" xml:lang="fr">fournit des attributs qui peuvent être utilisés pour
    associer une représentation telle qu'un nom ou un titre à l'information canonique concernant
    l'objet nommé ou auquel il est fait référence.</desc>
               <classes/>
               <attList>
                  <attDef ident="ref" mode="replace">
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="de"
                           versionDate="2023-03-28">Referenz auf die Datenbanken der SSRQ.</desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="en"
                           versionDate="2023-03-07">Reference to the databases of the SLS.</desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="fr"
                           versionDate="2023-05-16">Référence aux bases de données du SDS.</desc>
                     <datatype xmlns:sch="http://purl.oclc.org/dsdl/schematron">
                        <dataRef key="ssrq.pointer.databases"/>
                     </datatype>
                  </attDef>
               </attList>
               <listRef>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/ND.html#NDATTSnr">13.1.1. Linking Names and Their Referents</ref>
               </listRef>
            </classSpec>
            <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                       xmlns:xi="http://www.w3.org/2001/XInclude"
                       rend="change"
                       module="tei"
                       type="atts"
                       ident="att.dimensions">
               <desc versionDate="2007-08-05" xml:lang="en">provides attributes for describing the size of physical objects.</desc>
               <desc versionDate="2009-05-25" xml:lang="fr">fournit des attributs
  pour décrire la taille des objets    physiques</desc>
               <classes/>
               <attList>
                  <attDef ident="quantity" mode="replace">
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="de"
                           versionDate="2023-05-17">Gibt die Grösse der mit <att>unit</att> angegebenen
                        Einheit an
                    </desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="en"
                           versionDate="2023-05-17">Specifies the size of the unit specified with
                        <att>unit</att>
                     </desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="fr"
                           versionDate="2023-05-17">Spécifie la taille de l'unité spécifiée avec
                        <att>unit</att>
                     </desc>
                     <datatype xmlns:sch="http://purl.oclc.org/dsdl/schematron">
                        <rng:choice>
                           <rng:data type="float">
                              <rng:param name="minInclusive">0</rng:param>
                           </rng:data>
                           <rng:data type="positiveInteger"/>
                           <ref xmlns="http://relaxng.org/ns/structure/1.0" name="ssrq.measure.unknown"/>
                        </rng:choice>
                     </datatype>
                  </attDef>
                  <attDef ident="unit" mode="replace">
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="de"
                           versionDate="2023-03-17">Masseinheit</desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="en"
                           versionDate="2023-03-17">Unit of measurement</desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="fr"
                           versionDate="2023-03-17">Unité de mesure</desc>
                     <datatype xmlns:sch="http://purl.oclc.org/dsdl/schematron">
                        <dataRef key="ssrq.measure.lengths.base.cm"/>
                     </datatype>
                  </attDef>
               </attList>
            </classSpec>
            <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                       xmlns:xi="http://www.w3.org/2001/XInclude"
                       rend="change"
                       module="tei"
                       type="atts"
                       ident="att.written">
               <desc versionDate="2021-08-22" xml:lang="en">provides attributes to indicate the hand in which
    the content of an element was written in the source being transcribed.</desc>
               <classes/>
               <attList>
                  <attDef ident="hand" usage="opt">
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           rend="replace"
                           xml:lang="de"
                           versionDate="2023-08-22">Enthält einen Verweis auf die ID eines
                        <gi>handNote</gi>-Elements, das im <gi>teiHeader</gi> deklariert wurde.
              Die Einträge <val>laterHand</val> und <val>otherHand</val> haben unterschiedliche
              textkritische Relevanz. Vgl. hierzu die
              <ref target="emendation.md#behandlung-redaktioneller-eingriffe-des-schreibers">
                  Transkriptionsrichtlinien</ref>.
          </desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           rend="replace"
                           xml:lang="en"
                           versionDate="2023-08-22">Contains a reference to the ID of a <gi>handNote</gi> element
                declared in the <gi>teiHeader</gi>. The entries <val>laterHand</val> and <val>otherHand</val> have
                different text-critical relevance. See the transcription guidelines.
          </desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           rend="replace"
                           xml:lang="fr"
                           versionDate="2023-08-22">Contient une référence à l'ID d'un élément <gi>handNote</gi>
                déclaré dans le <gi>teiHeader</gi>. Les entrées <val>laterHand</val> et <val>otherHand</val> ont une
                pertinence différente pour le texte. Consultez les directives de transcription.
          </desc>
                     <datatype xmlns:sch="http://purl.oclc.org/dsdl/schematron">
                        <rng:choice>
                           <ref xmlns="http://relaxng.org/ns/structure/1.0" name="ssrq.hand"/>
                           <ref xmlns="http://relaxng.org/ns/structure/1.0" name="ssrq.hand.century"/>
                           <rng:data type="string"/>
                        </rng:choice>
                     </datatype>
                  </attDef>
               </attList>
               <listRef>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/ST.html#STECAT">1.3.1. Attribute Classes</ref>
               </listRef>
            </classSpec>
            <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                       xmlns:xi="http://www.w3.org/2001/XInclude"
                       rend="change"
                       module="tei"
                       type="atts"
                       ident="att.damaged">
               <desc versionDate="2007-09-05" xml:lang="en">provides attributes describing the nature of any physical damage affecting a reading.</desc>
               <desc versionDate="2008-03-30" xml:lang="fr">fournit des attributs décrivant la nature de tout dommage
    physique affectant la lecture.</desc>
               <classes/>
               <attList>
                  <attDef ident="agent" mode="replace" usage="req">
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="de"
                           versionDate="2023-05-11">Beschreibt die Art einer Beschädigung gemäss einer
                        fest definierten Werteliste.
                    </desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="en"
                           versionDate="2023-05-11">Describes the type of damage according to a firmly
                        defined list of values.
                    </desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="fr"
                           versionDate="2023-05-11">Décrit le type de dommage selon une liste de valeurs
                        bien définie.
                    </desc>
                     <datatype xmlns:sch="http://purl.oclc.org/dsdl/schematron" maxOccurs="unbounded">
                        <dataRef key="ssrq.agent"/>
                     </datatype>
                  </attDef>
               </attList>
               <listRef>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/PH.html#PHDA">11.3.3.1. Damage, Illegibility, and Supplied Text</ref>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/ST.html#STECAT">1.3.1. Attribute Classes</ref>
               </listRef>
            </classSpec>
            <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                       xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                       xmlns:xi="http://www.w3.org/2001/XInclude"
                       rend="change"
                       module="tei"
                       type="atts"
                       xml:id="DATABLE"
                       ident="att.datable">
               <desc versionDate="2007-06-12" xml:lang="en">provides attributes for normalization of elements
    that contain dates, times, or datable events.</desc>
               <desc versionDate="2007-06-12" xml:lang="fr">fournit des attributs pour la normalisation
    d'éléments qui contiennent des mentions d'événements datés ou susceptibles de l'être</desc>
               <classes/>
               <attList>
                  <attDef ident="calendar" mode="replace">
                     <desc xml:lang="de" versionDate="2023-09-12">verwendeter Kalender</desc>
                     <desc xml:lang="en" versionDate="2023-09-12">calendar used</desc>
                     <desc xml:lang="fr" versionDate="2023-09-12">calendrier utilisé</desc>
                     <datatype>
                        <dataRef key="ssrq.date.calendar"/>
                     </datatype>
                  </attDef>
                  <attDef ident="period" usage="opt">
                     <desc rend="replace" xml:lang="de" versionDate="2023-05-11">Verweist auf eine benannte Periode wie Jahreszeiten
                        oder Wochentage. Für Zeitspannen wie „vier Wochen“ wird hingegen das Attribut <att>dur-iso</att> verwendet.
                    </desc>
                     <desc rend="replace" xml:lang="en" versionDate="2023-05-11">Refers to a named period such as seasons or days of the
                        week. For periods such as "four weeks", on the other hand, the attribute <att>dur-iso</att> is
                        used.
                    </desc>
                     <desc rend="replace" xml:lang="fr" versionDate="2023-05-11">Fait référence à une période nommée, comme les saisons
                        ou les jours de la semaine. En revanche, l'attribut <att>dur-iso</att> est utilisé pour des
                        périodes telles que "quatre semaines".
                    </desc>
                     <datatype minOccurs="1" maxOccurs="unbounded">
                        <dataRef key="teidata.pointer"/>
                     </datatype>
                  </attDef>
               </attList>
               <listRef>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/CO.html#CONADA">3.6.4. Dates and Times</ref>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/ND.html#NDDATE">13.4. Dates</ref>
               </listRef>
            </classSpec>
            <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                       xmlns:xi="http://www.w3.org/2001/XInclude"
                       rend="change"
                       module="tei"
                       predeclare="true"
                       type="atts"
                       ident="att.global.responsibility">
               <desc versionDate="2014-12-10" xml:lang="en">provides attributes indicating the agent
    responsible for some aspect of the text, the markup or 
    something asserted by the markup, and the degree of certainty 
    associated with it.</desc>
               <classes/>
               <attList>
                  <attDef ident="cert" mode="replace">
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="de"
                           versionDate="2023-09-01">Angabe des Grads der Sicherheit; nur notwendig bei unsicherer Angabe</desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="en"
                           versionDate="2023-09-01">Indication of the degree of certainty; only necessary if the information is uncertain</desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="fr"
                           versionDate="2023-09-01">Indication du degré de certitude ; nécessaire uniquement en cas d'indication incertaine</desc>
                     <valList xmlns:sch="http://purl.oclc.org/dsdl/schematron" type="closed">
                        <valItem ident="high">
                           <desc xml:lang="de" versionDate="2023-04-11">hoch</desc>
                           <desc xml:lang="en" versionDate="2023-04-11">high</desc>
                           <desc xml:lang="fr" versionDate="2023-04-11">élevé</desc>
                        </valItem>
                        <valItem ident="medium">
                           <desc xml:lang="de" versionDate="2023-04-11">mittel</desc>
                           <desc xml:lang="en" versionDate="2023-04-11">medium</desc>
                           <desc xml:lang="fr" versionDate="2023-04-11">moyen</desc>
                        </valItem>
                        <valItem ident="low">
                           <desc xml:lang="de" versionDate="2023-04-11">niedrig</desc>
                           <desc xml:lang="en" versionDate="2022-05-06">low</desc>
                           <desc xml:lang="fr" versionDate="2023-04-11">faible</desc>
                        </valItem>
                     </valList>
                  </attDef>
                  <attDef ident="resp" usage="opt">
                     <gloss versionDate="2009-11-02" xml:lang="en">responsible party</gloss>
                     <gloss versionDate="2008-03-30" xml:lang="fr">responsable</gloss>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           rend="replace"
                           xml:lang="de"
                           versionDate="2023-12-18">Enthält die zwei- oder dreistelligen Kürzel der Bearbeitenden.</desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           rend="replace"
                           xml:lang="en"
                           versionDate="2023-12-18">Contains the two- or three-character abbreviations of the editors.</desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           rend="replace"
                           xml:lang="en"
                           versionDate="2023-12-18">Contient les abréviations à deux ou trois chiffres des éditeurs.</desc>
                     <datatype xmlns:sch="http://purl.oclc.org/dsdl/schematron">
                        <rng:list>
                           <rng:oneOrMore>
                              <rng:data type="string">
                                 <rng:param name="pattern">[A-Z]{2,3}</rng:param>
                              </rng:data>
                           </rng:oneOrMore>
                        </rng:list>
                     </datatype>
                  </attDef>
               </attList>
               <listRef>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/ST.html#STGAso">1.3.1.1.4. Sources, certainty, and responsibility</ref>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/CO.html#COED">3.5. Simple Editorial Changes</ref>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/PH.html#PHHR">11.3.2.2. Hand, Responsibility, and Certainty Attributes</ref>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/AI.html#AISP">17.3. Spans and Interpretations</ref>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/ND.html#NDATTSnr">13.1.1. Linking Names and Their Referents</ref>
               </listRef>
            </classSpec>
            <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                       xmlns:xi="http://www.w3.org/2001/XInclude"
                       rend="change"
                       predeclare="true"
                       module="tei"
                       type="atts"
                       ident="att.global.rendition">
               <desc versionDate="2014-12-02" xml:lang="en">provides rendering attributes common to all elements in the TEI encoding scheme.</desc>
               <classes/>
               <attList>
                  <attDef ident="rend" usage="opt">
                     <gloss versionDate="2007-07-02" xml:lang="en">rendition</gloss>
                     <gloss versionDate="2008-03-30" xml:lang="fr">interprétation</gloss>
                     <desc versionDate="2005-10-10" xml:lang="en">indicates how the element in question was rendered or presented in the source text.</desc>
                     <desc versionDate="2007-06-12" xml:lang="fr">indique comment l'élément en question a été rendu ou
        présenté dans le texte source</desc>
                     <datatype maxOccurs="unbounded">
                        <dataRef key="teidata.word"/>
                     </datatype>
                  </attDef>
               </attList>
               <listRef>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/ST.html#STGAre">1.3.1.1.3. Rendition Indicators</ref>
               </listRef>
            </classSpec>
            <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                       xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                       xmlns:xi="http://www.w3.org/2001/XInclude"
                       rend="change"
                       predeclare="true"
                       xml:id="GBLSRC"
                       module="tei"
                       type="atts"
                       ident="att.global.source">
               <desc versionDate="2021-08-22" xml:lang="en">provides attributes used by elements to point to an
    external source.</desc>
               <classes/>
               <constraintSpec scheme="schematron" ident="only_1_ODD_source">
                  <constraint>
                     <sch:rule context="tei:*[@source]">
                        <sch:let name="srcs" value="tokenize( normalize-space(@source),' ')"/>
                        <sch:report test="( self::tei:classRef               | self::tei:dataRef               | self::tei:elementRef               | self::tei:macroRef               | self::tei:moduleRef               | self::tei:schemaSpec )               and               $srcs[2]">
              When used on a schema description element (like
              <sch:value-of select="name(.)"/>), the @source attribute
              should have only 1 value. (This one has <sch:value-of select="count($srcs)"/>.)
            </sch:report>
                     </sch:rule>
                  </constraint>
               </constraintSpec>
               <attList>
                  <attDef ident="source" usage="opt">
                     <desc rend="replace" xml:lang="de" versionDate="2023-03-07">Verweis auf ein anderes Stück
                        (bspw. im Falle der Mehrfachüberlieferung) oder auf ein
                        Archivinformationssystem.
                </desc>
                     <desc rend="replace" xml:lang="en" versionDate="2023-03-07">Reference to another item (e.g. in
                        the case of multiple records) or to an archival information system.
                    </desc>
                     <desc rend="replace" xml:lang="fr" versionDate="2023-03-07">Renvoi à une autre pièce (par ex.
                        en cas de livraison multiple) ou à un système d'information archivistique.
                    </desc>
                     <constraintSpec scheme="schematron" ident="only_1_ODD_source">
                        <constraint>
                           <sch:rule context="tei:*[@source]">
                              <sch:let name="srcs" value="tokenize( normalize-space(@source),' ')"/>
                              <sch:report test="( self::tei:classRef               | self::tei:dataRef               | self::tei:elementRef               | self::tei:macroRef               | self::tei:moduleRef               | self::tei:schemaSpec )               and               $srcs[2]">
              When used on a schema description element (like
              <sch:value-of select="name(.)"/>), the @source attribute
              should have only 1 value. (This one has <sch:value-of select="count($srcs)"/>.)
            </sch:report>
                           </sch:rule>
                        </constraint>
                     </constraintSpec>
                     <datatype>
                        <rng:choice>
                           <rng:list>
                              <rng:oneOrMore>
                                 <ref xmlns="http://relaxng.org/ns/structure/1.0" name="ssrq.pointer.urn"/>
                              </rng:oneOrMore>
                           </rng:list>
                           <ref xmlns="http://relaxng.org/ns/structure/1.0" name="ssrq.pointer.url"/>
                        </rng:choice>
                     </datatype>
                  </attDef>
               </attList>
               <listRef>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/ST.html#STGAso">1.3.1.1.4. Sources, certainty, and responsibility</ref>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/CO.html#COHQQ">3.3.3. Quotation</ref>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/TS.html#TSBAWR">8.3.4. Writing</ref>
               </listRef>
            </classSpec>
            <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                       xmlns:xi="http://www.w3.org/2001/XInclude"
                       rend="change"
                       module="tei"
                       xml:id="GLOBAL"
                       type="atts"
                       ident="att.global">
               <desc versionDate="2007-10-02" xml:lang="en">provides attributes common to all elements in the TEI encoding scheme.</desc>
               <desc versionDate="2007-06-12" xml:lang="fr">fournit un jeu d'attributs communs à tous les éléments
    dans le système de codage TEI.</desc>
               <classes/>
               <attList>
                  <attDef ident="xml:lang" mode="replace">
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="de"
                           versionDate="2023-05-23">ISO-639-1-Sprachkürzel</desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="en"
                           versionDate="2023-05-23">ISO-639-1-Language abbreviation</desc>
                     <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xml:lang="fr"
                           versionDate="2023-05-23">ISO-639-1-Abréviation de la langue</desc>
                     <datatype xmlns:sch="http://purl.oclc.org/dsdl/schematron">
                        <rng:choice>
                           <ref xmlns="http://relaxng.org/ns/structure/1.0" name="ssrq.lang.de"/>
                           <ref xmlns="http://relaxng.org/ns/structure/1.0" name="ssrq.lang.fr"/>
                           <ref xmlns="http://relaxng.org/ns/structure/1.0" name="ssrq.lang.he"/>
                           <ref xmlns="http://relaxng.org/ns/structure/1.0" name="ssrq.lang.it"/>
                           <ref xmlns="http://relaxng.org/ns/structure/1.0" name="ssrq.lang.la"/>
                           <ref xmlns="http://relaxng.org/ns/structure/1.0" name="ssrq.lang.rm"/>
                        </rng:choice>
                     </datatype>
                  </attDef>
                  <attDef ident="xml:id" usage="opt">
                     <gloss versionDate="2007-07-02" xml:lang="en">identifier</gloss>
                     <gloss versionDate="2008-03-30" xml:lang="fr">identifiant</gloss>
                     <desc versionDate="2005-10-10" xml:lang="en">provides a unique identifier for the element bearing the attribute.</desc>
                     <desc versionDate="2009-05-25" xml:lang="fr">fournit un identifiant unique pour l'élément qui
        porte l'attribut</desc>
                     <datatype>
                        <dataRef name="ID"/>
                     </datatype>
                  </attDef>
