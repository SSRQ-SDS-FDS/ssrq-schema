               <gloss rend="replace" xml:lang="en" versionDate="2025-02-03">choice</gloss>
               <gloss rend="replace" xml:lang="fr" versionDate="2025-02-03">choix</gloss>
               <desc rend="replace" xml:lang="de" versionDate="2023-07-19">Enthält zwei Elemente zur alternativen Auszeichnung
    einer Textpassage.</desc>
               <desc rend="replace" xml:lang="en" versionDate="2023-07-19">Contains two elements for alternative markup of a
    text passage.</desc>
               <desc rend="replace" xml:lang="fr" versionDate="2023-07-19">Contient deux éléments pour le marquage alternatif
    d'un passage de texte.</desc>
               <classes>
                  <memberOf key="model.pPart.editorial"/>
               </classes>
               <content>
                  <alternate minOccurs="1" maxOccurs="1">
                     <sequence minOccurs="1" maxOccurs="1">
                        <elementRef key="abbr"/>
                        <elementRef key="expan"/>
                     </sequence>
                     <sequence minOccurs="1" maxOccurs="1">
                        <elementRef key="sic"/>
                        <elementRef key="corr"/>
                     </sequence>
                  </alternate>
               </content>
