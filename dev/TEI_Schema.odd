            <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                         rend="change"
                         module="figures"
                         ident="table">
               <gloss rend="replace" xml:lang="de" versionDate="2025-02-04">Tabelle</gloss>
               <gloss rend="replace" xml:lang="en" versionDate="2025-02-04">table</gloss>
               <gloss rend="replace" xml:lang="fr" versionDate="2025-02-04">tableau</gloss>
               <desc rend="replace" xml:lang="de" versionDate="2023-07-11">Enthält eine Tabelle.</desc>
               <desc rend="replace" xml:lang="en" versionDate="2023-07-11">Describes a table.</desc>
               <desc rend="replace" xml:lang="fr" versionDate="2023-07-11">Décrit un tableau.</desc>
               <classes/>
               <content>
                  <sequence minOccurs="1" maxOccurs="1" preserveOrder="false">
                     <alternate minOccurs="0" maxOccurs="unbounded">
                        <elementRef key="cb"/>
                        <elementRef key="head"/>
                        <elementRef key="pb"/>
                     </alternate>
                     <elementRef key="row" maxOccurs="unbounded"/>
                  </sequence>
               </content>
               <attList>
                  <attDef ident="rows" mode="delete"/>
