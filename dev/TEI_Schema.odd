               <gloss rend="replace" xml:lang="en" versionDate="2025-02-03">correction</gloss>
               <gloss rend="replace" xml:lang="fr" versionDate="2025-02-03">correction</gloss>
               <desc rend="replace" xml:lang="de" versionDate="2025-02-05">Enthält eine durch einen Editor bzw. eine Editorin
      vorgenommene Korrektur eines Schreibfehlers.</desc>
               <desc rend="replace" xml:lang="en" versionDate="2023-05-24">Contains a correction made by an editor.</desc>
               <desc rend="replace" xml:lang="fr" versionDate="2023-05-24">Contient une correction faite par un éditeur.</desc>
               <classes>
                  <memberOf key="att.global.responsibility"/>
                  <memberOf key="model.pPart.transcriptional"/>
               </classes>
