            <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                         xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                         rend="change"
                         module="certainty"
                         ident="precision">
               <gloss rend="replace" xml:lang="de" versionDate="2025-02-04">Genauigkeit</gloss>
               <gloss rend="replace" xml:lang="en" versionDate="2025-02-04">precision</gloss>
               <gloss rend="replace" xml:lang="fr" versionDate="2025-02-04">précision</gloss>
               <desc rend="replace" xml:lang="de" versionDate="2023-07-14">Gibt Unsicherheit bei Datierungen an.</desc>
               <desc rend="replace" xml:lang="en" versionDate="2023-07-14">Indicates uncertainty in dating.</desc>
               <desc rend="replace" xml:lang="fr" versionDate="2023-07-14">Indique une incertitude dans la datation.</desc>
               <classes>
                  <memberOf key="att.scoping"/>
                  <memberOf key="model.global.meta"/>
               </classes>
               <content>
                  <empty/>
               </content>
               <constraintSpec xml:lang="en" scheme="schematron" ident="sch-el-precision">
                  <desc xml:lang="en" versionDate="2023-03-07">constraint for tei:precision</desc>
                  <constraint>
                     <sch:pattern>
                        <sch:rule context="tei:precision[@match]">
                           <sch:let name="parent" value="./parent::*"/>
                           <sch:assert test="every $matchAttr in tokenize(./@match, ' ') satisfies exists($parent/@*[name() = substring-after($matchAttr, '@')])">
              Every value that is targeted via @match must have a corresponding attribute on the
              parent element.
          </sch:assert>
                        </sch:rule>
                     </sch:pattern>
                  </constraint>
               </constraintSpec>
               <attList>
                  <attDef ident="calendar" mode="delete"/>
                  <attDef ident="precision" usage="req">
                     <desc rend="replace" xml:lang="de" versionDate="2025-02-03">die Präzision des in <att>match</att> genannten Attributs</desc>
                     <desc rend="replace" xml:lang="en" versionDate="2025-02-03">the precision of the attribute mentioned in <att>match</att>
                     </desc>
                     <desc rend="replace" xml:lang="fr" versionDate="2025-02-03">la précision de l'attribut mentionné dans <att>match</att>
                     </desc>
                     <datatype>
                        <dataRef key="teidata.certainty"/>
                     </datatype>
                  </attDef>
                  <attDef ident="stdDeviation" mode="delete"/>
               </attList>
               <exemplum xml:lang="de" xml:id="ex-precision-de">
                  <p>Beispiel für ein unsicheres Datum</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples">
                     <date calendar="julian"
                           notBefore-custom="1341-01-01"
                           notAfter-custom="1355-12-31">
        Um 1346 und um 1350
        <precision precision="low" match="@notBefore-custom @notAfter-custom"/>
                     </date>
                  </egXML>
               </exemplum>
               <exemplum xml:lang="en" xml:id="ex-precision-en">
                  <p>Example of an uncertain date</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples">
                     <date calendar="julian"
                           notBefore-custom="1341-01-01"
                           notAfter-custom="1355-12-31">
        Um 1346 und um 1350
        <precision precision="low" match="@notBefore-custom @notAfter-custom"/>
                     </date>
                  </egXML>
               </exemplum>
               <exemplum xml:lang="fr" xml:id="ex-precision-fr">
                  <p>Exemple de date incertaine</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples">
                     <date calendar="julian"
                           notBefore-custom="1341-01-01"
                           notAfter-custom="1355-12-31">
        Um 1346 und um 1350
        <precision precision="low" match="@notBefore-custom @notAfter-custom"/>
                     </date>
                  </egXML>
               </exemplum>
               <remarks xml:lang="de" versionDate="2025-02-06">
                  <p>Mit dem Attribut <att>match</att> wird auf ein oder mehrere Attribute von <gi>date</gi>
      Bezug genommen. Mit dem Attribut <att>precision</att> wird der Grad der Unsicherheit
      angegeben.</p>
               </remarks>
               <remarks xml:lang="en" versionDate="2023-07-14">
                  <p>
                     <att>match</att> is used to refer to one or more attributes of <gi>date</gi>.
        <att>precision</att> specifies the degree of uncertainty.</p>
               </remarks>
               <remarks xml:lang="fr" versionDate="2023-07-14">
                  <p>
                     <att>match</att> est utilisé pour faire référence à un ou plusieurs attributs de
        <gi>date</gi>. <att>precision</att> spécifie le degré d'incertitude.</p>
               </remarks>
               <listRef>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/CE.html#CEPREC">22.2. Indications of Precision</ref>
               </listRef>
            </elementSpec>
