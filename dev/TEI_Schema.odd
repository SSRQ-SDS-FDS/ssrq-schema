                  <egXML xmlns="http://www.tei-c.org/ns/Examples"
                         xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                         xmlns:xi="http://www.w3.org/2001/XInclude">
                     <ref target="urn:ssrq:SSRQ-ZH-NF_I_2_1-4-1">SSRQ ZH NF I/2/1 4-1</ref>
                  </egXML>
