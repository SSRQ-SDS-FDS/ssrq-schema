            <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                         rend="change"
                         module="certainty"
                         ident="precision">
               <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     xmlns:xi="http://www.w3.org/2001/XInclude"
                     rend="replace"
                     xml:lang="de"
                     versionDate="2023-07-14">Gibt Unsicherheit bei Datierungen an.</desc>
               <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     xmlns:xi="http://www.w3.org/2001/XInclude"
                     rend="replace"
                     xml:lang="en"
                     versionDate="2023-07-14">Indicates uncertainty in dating.</desc>
               <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     xmlns:xi="http://www.w3.org/2001/XInclude"
                     rend="replace"
                     xml:lang="fr"
                     versionDate="2023-07-14">Indique une incertitude dans la datation.</desc>
               <classes>
                  <memberOf xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                            xmlns:xi="http://www.w3.org/2001/XInclude"
                            key="att.scoping"/>
                  <memberOf xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                            xmlns:xi="http://www.w3.org/2001/XInclude"
                            key="model.global.meta"/>
               </classes>
               <content>
                  <empty xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                         xmlns:xi="http://www.w3.org/2001/XInclude"/>
               </content>
               <constraintSpec xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                               xmlns:xi="http://www.w3.org/2001/XInclude"
                               scheme="schematron"
                               ident="sch-el-precision">
                  <desc xml:lang="en" versionDate="2023-03-07">Constraint for tei:precision test-matches</desc>
                  <constraint>
                     <sch:pattern>
                        <sch:rule context="tei:precision[@match]">
                           <sch:let name="parent" value="./parent::*"/>
                           <sch:assert test="every $matchAttr in tokenize(./@match, ' ') satisfies exists($parent/@*[name() = substring-after($matchAttr, '@')])">
                        Every value that is targeted via @match must have a corresponding attribute on the parent.
                    </sch:assert>
                        </sch:rule>
                     </sch:pattern>
                  </constraint>
               </constraintSpec>
               <attList>
                  <attDef ident="precision" usage="req">
                     <desc versionDate="2011-04-13" xml:lang="en">characterizes the precision of the element or attribute pointed 
        to by the <gi>precision</gi> element.</desc>
                     <datatype>
                        <dataRef key="teidata.certainty"/>
                     </datatype>
                  </attDef>
                  <attDef xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                          xmlns:xi="http://www.w3.org/2001/XInclude"
                          ident="stdDeviation"
                          mode="delete"/>
               </attList>
               <exemplum xml:lang="de" xml:id="ex-precision-de">
                  <p xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     xmlns:xi="http://www.w3.org/2001/XInclude">Beispiel für ein unsicheres Datum</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples"
                         xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                         xmlns:xi="http://www.w3.org/2001/XInclude">
                     <date calendar="julian"
                           notBefore-custom="1341-01-01"
                           notAfter-custom="1355-12-31">
        Um 1346 und um 1350
        <precision precision="low" match="@notBefore-custom @notAfter-custom"/>
                     </date>
                  </egXML>
               </exemplum>
               <exemplum xml:lang="en" xml:id="ex-precision-en">
                  <p xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     xmlns:xi="http://www.w3.org/2001/XInclude">Example of an uncertain date</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples"
                         xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                         xmlns:xi="http://www.w3.org/2001/XInclude">
                     <date calendar="julian"
                           notBefore-custom="1341-01-01"
                           notAfter-custom="1355-12-31">
        Um 1346 und um 1350
        <precision precision="low" match="@notBefore-custom @notAfter-custom"/>
                     </date>
                  </egXML>
               </exemplum>
               <exemplum xml:lang="fr" xml:id="ex-precision-fr">
                  <p xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     xmlns:xi="http://www.w3.org/2001/XInclude">Exemple de date incertaine</p>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples"
                         xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                         xmlns:xi="http://www.w3.org/2001/XInclude">
                     <date calendar="julian"
                           notBefore-custom="1341-01-01"
                           notAfter-custom="1355-12-31">
        Um 1346 und um 1350
        <precision precision="low" match="@notBefore-custom @notAfter-custom"/>
                     </date>
                  </egXML>
               </exemplum>
               <remarks xml:lang="de" versionDate="2023-07-14">
                  <p xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     xmlns:xi="http://www.w3.org/2001/XInclude">Mit <att>match</att> wird auf ein oder mehrere Attribute von <gi>date</gi> Bezug genommen.
            Mit <att>precision</att> wird der Grad der Unsicherheit angegeben.
        </p>
               </remarks>
               <remarks xml:lang="en" versionDate="2023-07-14">
                  <p xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     xmlns:xi="http://www.w3.org/2001/XInclude">
                     <att>match</att>
            is used to refer to one or more attributes of <gi>date</gi>. <att>precision</att> specifies the degree of
            uncertainty.
        </p>
               </remarks>
               <remarks xml:lang="fr" versionDate="2023-07-14">
                  <p xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                     xmlns:xi="http://www.w3.org/2001/XInclude">
                     <att>match</att>
            est utilisé pour faire référence à un ou plusieurs attributs de <gi>date</gi>. <att>precision</att> spécifie
            le degré d'incertitude.
        </p>
               </remarks>
               <listRef>
                  <ref target="https://www.tei-c.org/release/doc/tei-p5-doc/en/html/CE.html#CEPREC">21.2. Indications of Precision</ref>
               </listRef>
            </elementSpec>
