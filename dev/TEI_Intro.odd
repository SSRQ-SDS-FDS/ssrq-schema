                        <desc xml:lang="de" versionDate="2023-03-15">Stammbaum</desc>
                        <desc xml:lang="en" versionDate="2023-03-15">familytree</desc>
                        <desc xml:lang="fr" versionDate="2023-03-15">pédigrée</desc>
                        <desc xml:lang="it" versionDate="2023-03-15">albero genealogico</desc>
                     </valItem>
                     <valItem ident="figure">
                        <desc xml:lang="de" versionDate="2023-03-15">Visualisierung</desc>
                        <desc xml:lang="en" versionDate="2023-03-15">visualisation</desc>
                        <desc xml:lang="fr" versionDate="2023-03-15">visualisation</desc>
                        <desc xml:lang="it" versionDate="2023-03-15">visualizzazione</desc>
                     </valItem>
                     <valItem ident="graphic">
                        <desc xml:lang="de" versionDate="2023-03-15">Illustration</desc>
                        <desc xml:lang="en" versionDate="2023-03-15">illustration</desc>
                        <desc xml:lang="fr" versionDate="2023-03-15">illustration</desc>
                        <desc xml:lang="it" versionDate="2023-03-15">illustrazione</desc>
                     </valItem>
                     <valItem ident="map">
                        <desc xml:lang="de" versionDate="2023-03-15">Karte</desc>
                        <desc xml:lang="en" versionDate="2023-03-15">map</desc>
                        <desc xml:lang="fr" versionDate="2023-03-15">carte</desc>
                        <desc xml:lang="it" versionDate="2023-03-15">mappa</desc>
                     </valItem>
                  </valList>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      module="derived-module-ssrq-intro-schema"
                      ident="ssrq.type.head"
                      mode="add">
               <content xmlns:rng="http://relaxng.org/ns/structure/1.0">
                  <valList type="closed">
                     <valItem ident="title">
                        <desc xml:lang="de" versionDate="2023-05-26">Haupttitel</desc>
                        <desc xml:lang="en" versionDate="2023-05-26">title</desc>
                        <desc xml:lang="fr" versionDate="2023-05-26">titre principal</desc>
                        <desc xml:lang="it" versionDate="2024-05-08">titolo principale</desc>
                     </valItem>
                     <valItem ident="subtitle">
                        <desc xml:lang="de" versionDate="2023-05-26">Untertitel</desc>
                        <desc xml:lang="en" versionDate="2023-05-26">subtitle</desc>
                        <desc xml:lang="fr" versionDate="2023-05-26">sous-titre</desc>
                        <desc xml:lang="it" versionDate="2024-05-08">sottotitolo</desc>
                     </valItem>
                     <valItem ident="subsubtitle">
                        <desc xml:lang="de" versionDate="2023-05-26">Unteruntertitel</desc>
                        <desc xml:lang="en" versionDate="2023-05-26">subsubtitle</desc>
                        <desc xml:lang="fr" versionDate="2023-05-26">sous-sous-titre</desc>
                        <desc xml:lang="it" versionDate="2024-05-08">sottosottotitolo</desc>
                     </valItem>
                  </valList>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      module="derived-module-ssrq-intro-schema"
                      ident="ssrq.type.idno"
                      mode="add">
               <content xmlns:rng="http://relaxng.org/ns/structure/1.0">
                  <valList type="closed">
                     <valItem ident="uuid">
                        <desc xml:lang="de" versionDate="2023-03-03">Universally Unique Identifier</desc>
                        <desc xml:lang="en" versionDate="2023-03-03">Universally Unique Identifier</desc>
                        <desc xml:lang="fr" versionDate="2023-03-03">Universally Unique Identifier</desc>
                        <desc xml:lang="it" versionDate="2023-03-03">Universally Unique Identifier</desc>
                     </valItem>
                  </valList>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      module="derived-module-ssrq-intro-schema"
                      ident="ssrq.type.label"
                      mode="add">
               <content xmlns:rng="http://relaxng.org/ns/structure/1.0">
                  <valList type="closed">
                     <valItem ident="keyword">
                        <desc xml:lang="de" versionDate="2023-05-25">Schlüsselwort</desc>
                        <desc xml:lang="en" versionDate="2023-05-25">keyword</desc>
                        <desc xml:lang="fr" versionDate="2023-05-25">mot-clé</desc>
                        <desc xml:lang="it" versionDate="2023-05-25">parola chiave</desc>
                     </valItem>
                  </valList>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      module="derived-module-ssrq-intro-schema"
                      ident="ssrq.type.pb"
