               <gloss xmlns:xi="http://www.w3.org/2001/XInclude"
                      rend="replace"
                      xml:lang="de"
                      versionDate="2025-02-03">Datum</gloss>
               <gloss xmlns:xi="http://www.w3.org/2001/XInclude"
                      rend="replace"
                      xml:lang="en"
                      versionDate="2025-02-03">date</gloss>
               <gloss xmlns:xi="http://www.w3.org/2001/XInclude"
                      rend="replace"
                      xml:lang="fr"
                      versionDate="2025-02-03">date</gloss>
               <desc xmlns:xi="http://www.w3.org/2001/XInclude"
                     rend="replace"
                     xml:lang="de"
                     versionDate="2023-08-17">Enthält ein Datum.</desc>
               <desc xmlns:xi="http://www.w3.org/2001/XInclude"
                     rend="replace"
                     xml:lang="en"
                     versionDate="2023-08-17">Contains a date.</desc>
               <desc xmlns:xi="http://www.w3.org/2001/XInclude"
                     rend="replace"
                     xml:lang="fr"
                     versionDate="2023-08-17">Contient une date.</desc>
               <classes>
                  <memberOf xmlns:xi="http://www.w3.org/2001/XInclude" key="att.calendarSystem"/>
                  <memberOf xmlns:xi="http://www.w3.org/2001/XInclude" key="att.datable"/>
                  <memberOf xmlns:xi="http://www.w3.org/2001/XInclude" key="att.datable.custom"/>
                  <memberOf xmlns:xi="http://www.w3.org/2001/XInclude" key="att.duration.iso"/>
                  <memberOf xmlns:xi="http://www.w3.org/2001/XInclude" key="att.typed"/>
               </classes>
               <content>
                  <alternate minOccurs="0" maxOccurs="unbounded">
                     <elementRef xmlns:xi="http://www.w3.org/2001/XInclude" key="precision"/>
                     <macroRef xmlns:xi="http://www.w3.org/2001/XInclude" key="ssrq.content.default"/>
                  </alternate>
               </content>
               <constraintSpec xmlns:xi="http://www.w3.org/2001/XInclude"
                               xml:lang="en"
                               scheme="schematron"
                               ident="sch-el-date-outside-teiHeader"
                               mode="add">
                  <desc xml:lang="en" versionDate="2025-02-08">constraint for tei:date outside tei:teiHeader</desc>
                  <constraint>
                     <sch:pattern>
                        <sch:rule context="tei:date[not(ancestor::tei:teiHeader)]">
                           <sch:assert test="string-length(normalize-space(.)) &gt; 0 or child::*">
              The element tei:date must not be empty when outside tei:teiHeader.
          </sch:assert>
                        </sch:rule>
                     </sch:pattern>
                  </constraint>
               </constraintSpec>
               <constraintSpec xmlns:xi="http://www.w3.org/2001/XInclude"
                               xml:lang="en"
                               scheme="schematron"
                               ident="sch-el-date-publicationStmt"
                               mode="add">
                  <desc xml:lang="en" versionDate="2025-02-08">constraint for tei:date inside tei:publicationStmt</desc>
                  <constraint>
                     <sch:pattern>
                        <sch:rule context="tei:date[ancestor::tei:publicationStmt]">
                           <sch:assert test="string-length(normalize-space(.)) = 0 and not(child::*)">
            If tei:date is part of tei:publicationStmt it must be empty.
          </sch:assert>
                           <sch:assert test=".[@type][@type/data(.) = ('electronic', 'print')]">
            If tei:date is part of tei:publicationStmt it must contain @type with a value of
            'electronic' or 'print'.
          </sch:assert>
                           <sch:assert test=".[@when-custom or (@from-custom and @to-custom)][not(@calendar|@dur-iso|@notAfter-custom|@notBefore-custom|@period)]">
            If tei:date is part of tei:publicationStmt it must contain @when-custom or @from-custom
            and to-custom, and may not contain @calendar, @dur-iso, @notAfter-custom,
            @notBefore-custom or @period.
          </sch:assert>
                        </sch:rule>
                     </sch:pattern>
                  </constraint>
               </constraintSpec>
               <constraintSpec xmlns:xi="http://www.w3.org/2001/XInclude"
                               xml:lang="en"
                               scheme="schematron"
                               ident="sch-el-date-electronic-print"
                               mode="add">
                  <desc xml:lang="en" versionDate="2025-02-17">constraint for tei:date with type 'electronic'
      and 'print' outside tei:publicationStmt</desc>
                  <constraint>
                     <sch:pattern>
                        <sch:rule context="tei:date[not(ancestor::tei:publicationStmt)]">
                           <sch:report test=".[@type='electronic' or @type='print']">
            If tei:date is outside tei:publicationStmt it may not be of @type 'electronic'
            or 'print'.
          </sch:report>
                        </sch:rule>
                     </sch:pattern>
                  </constraint>
