   <define name="att.datable.custom.attributes">
      <ref name="att.datable.custom.attribute.from-custom"/>
      <ref name="att.datable.custom.attribute.notAfter-custom"/>
      <ref name="att.datable.custom.attribute.notBefore-custom"/>
      <ref name="att.datable.custom.attribute.to-custom"/>
      <ref name="att.datable.custom.attribute.when-custom"/>
   </define>
   <define name="att.datable.custom.attribute.from-custom">
      <optional>
         <attribute name="from-custom">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indicates the start point of a period according to ISO 8601. </a:documentation>
            <data type="string">
               <param name="pattern">(((\d{4}|-)-(0[1-9]|1[0-2]))|--)-(0[1-9]|[1-2][0-9]|3[0-1])</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="att.datable.custom.attribute.notAfter-custom">
      <optional>
         <attribute name="notAfter-custom">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indicates the end point of a period according to ISO 8601. </a:documentation>
            <data type="string">
               <param name="pattern">(((\d{4}|-)-(0[1-9]|1[0-2]))|--)-(0[1-9]|[1-2][0-9]|3[0-1])</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="att.datable.custom.attribute.notBefore-custom">
      <optional>
         <attribute name="notBefore-custom">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indicates the start point of a period according to ISO 8601. </a:documentation>
            <data type="string">
               <param name="pattern">(((\d{4}|-)-(0[1-9]|1[0-2]))|--)-(0[1-9]|[1-2][0-9]|3[0-1])</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="att.datable.custom.attribute.to-custom">
      <optional>
         <attribute name="to-custom">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indicates the end point of a period according to ISO 8601. </a:documentation>
            <data type="string">
               <param name="pattern">(((\d{4}|-)-(0[1-9]|1[0-2]))|--)-(0[1-9]|[1-2][0-9]|3[0-1])</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="att.datable.custom.attribute.when-custom">
      <optional>
         <attribute name="when-custom">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Specifies a date according to ISO 8601.</a:documentation>
            <data type="string">
               <param name="pattern">(((\d{4}|-)-(0[1-9]|1[0-2]))|--)-(0[1-9]|[1-2][0-9]|3[0-1])</param>
            </data>
         </attribute>
      </optional>
   </define>
   <sch:pattern xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:xi="http://www.w3.org/2001/XInclude">
      <sch:rule context="tei:*[@when-custom]">
         <sch:report test="@notBefore-custom|@notAfter-custom|@from-custom|@to-custom">The
                                @when-custom attribute cannot be used with any other
                                att.datable.custom attributes.
                            </sch:report>
      </sch:rule>
      <sch:rule context="tei:*[@from-custom or @to-custom]">
         <sch:report test="@notBefore-custom|@notAfter-custom">The usage of
                                @notBefore-custom|@notAfter-custom is not allowed together with
                                @from-custom|@to-custom.
                            </sch:report>
         <sch:assert test="@from-custom and @to-custom">@from-custom and
                                @to-custom must always be used together.
                            </sch:assert>
         <sch:assert test="if (matches(@from-custom, '^\d{4}-\d{2}-\d{2}$') and matches(@from-custom, '^\d{4}-\d{2}-\d{2}$')) then (number(translate(@to-custom, '-', '')) - number(translate(@from-custom, '-', ''))) &gt; 0 else true()">
                                Invalid timespan – the @to-custom attribute must be a point later in time.
                            </sch:assert>
      </sch:rule>
   </sch:pattern>
