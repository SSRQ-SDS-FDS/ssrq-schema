   <define name="att.dimensions.attribute.quantity">
      <optional>
         <attribute name="quantity">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Specifies the size of the unit specified with <code xmlns="http://www.w3.org/1999/xhtml">@unit</code>
            </a:documentation>
            <choice>
               <data type="float">
                  <param name="minInclusive">0</param>
               </data>
               <data type="positiveInteger"/>
