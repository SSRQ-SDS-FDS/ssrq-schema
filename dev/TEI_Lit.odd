               <desc xmlns:rng="http://relaxng.org/ns/structure/1.0"
                     xml:lang="fr"
                     versionDate="2025-01-30">les tâches des participants au projet</desc>
               <content xmlns:rng="http://relaxng.org/ns/structure/1.0">
                  <valList type="closed" xml:lang="fr">
                     <valItem ident="Contrôle de qualité"/>
                     <valItem ident="Encodage XML"/>
                     <valItem ident="Publication"/>
                  </valList>
               </content>
            </macroSpec>
            <dataSpec rend="add"
                      module="derived-module-ssrq-lit-schema"
                      ident="ssrq.target.licence"
                      mode="add">
               <content xmlns:rng="http://relaxng.org/ns/structure/1.0">
                  <valList type="closed">
                     <valItem ident="https://creativecommons.org/licenses/by-nc-sa/4.0/">
                        <desc xml:lang="de" versionDate="2025-02-03">der Link zu den Lizenzbedingungen</desc>
                        <desc xml:lang="en" versionDate="2025-02-03">the link to the license conditions</desc>
                        <desc xml:lang="fr" versionDate="2025-02-03">le lien vers les conditions de licence</desc>
                     </valItem>
                  </valList>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      module="derived-module-ssrq-lit-schema"
                      ident="ssrq-lit.type.date"
                      mode="add">
               <content xmlns:rng="http://relaxng.org/ns/structure/1.0">
                  <valList type="closed">
                     <valItem ident="electronic">
                        <desc xml:lang="de" versionDate="2024-05-08">elektronische Publikation</desc>
                        <desc xml:lang="en" versionDate="2024-05-08">electronic publication</desc>
                        <desc xml:lang="fr" versionDate="2024-05-08">publication numérique</desc>
                        <desc xml:lang="it" versionDate="2024-05-08">pubblicazione digitale</desc>
                     </valItem>
                     <valItem ident="print">
                        <desc xml:lang="de" versionDate="2024-05-08">Druck</desc>
                        <desc xml:lang="en" versionDate="2024-05-08">print</desc>
                        <desc xml:lang="fr" versionDate="2024-05-08">imprimé</desc>
                        <desc xml:lang="it" versionDate="2024-05-08">stampa</desc>
                     </valItem>
                  </valList>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      module="derived-module-ssrq-lit-schema"
                      ident="ssrq.type.idno"
                      mode="add">
               <content xmlns:rng="http://relaxng.org/ns/structure/1.0">
                  <valList type="closed">
                     <valItem ident="uuid">
                        <desc xml:lang="de" versionDate="2023-03-03">Universally Unique Identifier</desc>
                        <desc xml:lang="en" versionDate="2023-03-03">Universally Unique Identifier</desc>
                        <desc xml:lang="fr" versionDate="2023-03-03">Universally Unique Identifier</desc>
                        <desc xml:lang="it" versionDate="2023-03-03">Universally Unique Identifier</desc>
                     </valItem>
                  </valList>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      module="derived-module-ssrq-lit-schema"
                      ident="ssrq-lit.text.type"
                      mode="add">
               <content xmlns:rng="http://relaxng.org/ns/structure/1.0">
                  <valList type="closed">
                     <valItem ident="literature"/>
                  </valList>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.date.point"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für Datumsangaben</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for dates</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour les dates</desc>
               <content>
                  <dataRef name="string"
                           restriction="(((\d{4}|-)-(0[1-9]|1[0-2]))|--)-(0[1-9]|[1-2][0-9]|3[0-1])"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.foliation"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für die Blattzählung</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for the foliation</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour le foliation</desc>
               <content>
                  <dataRef name="string"
                           restriction="((([1-9]\d*)|([IVXLCDM]+))([ab]|bis)?[rv])|[rv]"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pagination"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für die Seitenzählung</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for the pagination</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour le pagination</desc>
               <content>
                  <dataRef name="string"
                           restriction="(([1-9]\d*(\.[1-9]\d*)?|([IVXLCDM]+))([ab]|bis)?)|cover"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.section"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für die Abschnittszählung</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for the section numbering</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour la numérotation des sections</desc>
               <content>
                  <dataRef name="string" restriction="s([1-9]\d*)"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.keywords"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für Schlagwort-IDs</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for keyword IDs</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour les ID de mots-clés</desc>
               <content>
                  <dataRef name="string" restriction="key\d{6}"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.lemma"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für Lemma-IDs</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for lemma IDs</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour les ID de lemmes</desc>
               <content>
                  <dataRef name="string" restriction="lem\d{6}(\.1?\d{2})?"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.org"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für Organisations-IDs</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for organisation IDs</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour les ID de organisations</desc>
               <content>
                  <dataRef name="string" restriction="org\d{6}(\.\d{2})?"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.persons"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für Personen-IDs</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for person IDs</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour les ID de personne</desc>
               <content>
                  <dataRef name="string" restriction="per\d{6}[abc]?(\.\d{2})?"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.places"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für Ort-IDs</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for place IDs</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour les ID de lieux</desc>
               <content>
                  <dataRef name="string" restriction="loc\d{6}(\.\d{2})?"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.databases"
                      mode="add"
                      module="ssrq.core.module">
               <content>
                  <alternate minOccurs="1" maxOccurs="1">
                     <dataRef key="ssrq.pointer.keywords"/>
                     <dataRef key="ssrq.pointer.lemma"/>
                     <dataRef key="ssrq.pointer.org"/>
                     <dataRef key="ssrq.pointer.persons"/>
                     <dataRef key="ssrq.pointer.places"/>
                  </alternate>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.doi"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für eine DOI</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for a DOI.</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour une DOI.</desc>
               <content>
                  <dataRef name="string" restriction="doi:10\.\d{4}/.*"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.url"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für eine URL</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for a URL</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour une URL</desc>
               <content>
                  <dataRef name="anyURI" restriction="(https?|ftp)://[^\s/$.?#].[^\s]*"/>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.urn"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für eine URN</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for a URN</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour une URN</desc>
               <content>
                  <alternate minOccurs="1" maxOccurs="1">
                     <dataRef name="string"
                              restriction="urn:ssrq:(SSRQ|SDS|FDS)-([A-Z]{2})-([A-Za-z0-9_]+)-(((([A-Za-z0-9]+\.)*)([0-9]+)-(1))|(lit|intro|bailiffs))(#(\d+|fol\d+[rv](\-\d+[rv])?|[pn]\d+(\.\d+)?(\-\d+(\.\d+)?)?))?"/>
                     <dataRef name="string"
                              restriction="urn:ssrq:[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[4][0-9A-Fa-f]{3}-[89ABab][0-9A-Fa-f]{3}-[0-9A-Fa-f]{12}(#(\d+|fol\d+[rv](\-\d+[rv])?|[pn]\d+(\.\d+)?(\-\d+(\.\d+)?)?))?"/>
                  </alternate>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.pointer.target"
                      mode="add"
                      module="ssrq.core.module">
               <content>
                  <alternate minOccurs="1" maxOccurs="1">
                     <dataRef key="ssrq.pointer.doi"/>
                     <dataRef key="ssrq.pointer.url"/>
                     <dataRef key="ssrq.pointer.urn"/>
                  </alternate>
               </content>
            </dataSpec>
            <dataSpec rend="add"
                      ident="ssrq.id.string"
                      mode="add"
                      module="ssrq.core.module">
               <desc xml:lang="de" versionDate="2025-01-30">das Muster für eine SSRQ UUID</desc>
               <desc xml:lang="en" versionDate="2025-01-30">the pattern for an SLS UUID</desc>
               <desc xml:lang="fr" versionDate="2025-01-30">le modèle pour un UUID SDS</desc>
               <content>
                  <dataRef name="string"
                           restriction="id-ssrq-[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[4][0-9A-Fa-f]{3}-[89ABab][0-9A-Fa-f]{3}-[0-9A-Fa-f]{12}"/>
               </content>
            </dataSpec>
            <moduleSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                        xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                        xmlns:xi="http://www.w3.org/2001/XInclude"
                        n="ST"
                        xml:id="DSTTEI2"
                        ident="tei"
                        type="core"/>
            <moduleSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                        xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                        n="CO"
                        xml:id="DCO"
                        ident="core"/>
            <moduleSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                        xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                        xmlns:xi="http://www.w3.org/2001/XInclude"
                        n="HD"
                        xml:id="DHD"
                        ident="header"/>
            <moduleSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                        xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                        xmlns:xi="http://www.w3.org/2001/XInclude"
                        n="ND"
                        xml:id="DND"
                        ident="namesdates"/>
            <moduleSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
