<TEI xmlns="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">

    <xi:include href="templates/meta.odd.xml"/>

    <text>
        <body>
            <schemaSpec ident="ssrq-main-schema" docLang="de en fr it" xml:lang="en">

                <!-- TEI modules used -->
                <moduleRef key="tei"/>
                <moduleRef key="certainty"
                           include="precision"/>
                <moduleRef key="core"
                           include="abbr add author bibl cb choice corr date del editor expan foreign gap graphic head
                           hi item label lb list listBibl measure measureGrp note num orig p pb publisher pubPlace q
                           quote ref resp respStmt sic term textLang time title unclear"/>
                <moduleRef key="header"
                           include="availability editorialDecl encodingDesc extent fileDesc handNote idno keywords
                           licence profileDesc publicationStmt seriesStmt sourceDesc teiHeader textClass titleStmt"/>
                <moduleRef key="textstructure"
                           include="back body div docImprint signed TEI text"/>
                <moduleRef key="linking"
                           include="ab anchor seg"/>
                <moduleRef key="msdescription"
                           include="additional altIdentifier bindingDesc condition dimensions handDesc height history
                           material msContents msDesc msIdentifier msItem objectDesc origDate origin origPlace physDesc
                           repository seal sealDesc summary support supportDesc width"/>
                <moduleRef key="transcr"
                           include="addSpan damage damageSpan delSpan fw handShift space subst supplied"/>
                <moduleRef key="textcrit"
                           include="app lem listWit rdg witness"/>
                <moduleRef key="namesdates"
                           include="orgName persName placeName settlement"/>
                <moduleRef key="figures"
                           include="cell figure row table"/>

                <!-- own modules -->
                <specGrpRef target="common/modules.odd.xml#ssrq-modules"/>

                <!-- attribute classes -->
                <specGrpRef target="common/classes.odd.xml#ssrq-classes"/>

                <!-- global constraints -->
                <specGrpRef target="common/constraints.odd.xml#constraints"/>

                <!-- content -->
                <specGrpRef target="common/content.odd.xml#ssrq-default-content-types"/>

                <!-- datatypes -->
