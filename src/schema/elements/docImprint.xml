<elementSpec xmlns="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" ident="docImprint" module="textstructure" mode="change">
  <gloss xml:lang="de" versionDate="2023-07-31">Impressum</gloss>
  <gloss xml:lang="en" versionDate="2025-02-03">document imprint</gloss>
  <gloss xml:lang="fr" versionDate="2023-07-31">imprimer</gloss>
  <desc xml:lang="de" versionDate="2023-04-05">Enthält bei Druckschriften den Druckort
    (<gi>pubPlace</gi>) und den Namen der Druckerei (<gi>publisher</gi>).</desc>
  <desc xml:lang="en" versionDate="2023-04-05">Contains the place of printing (<gi>pubPlace</gi>)
    and the name of the printer (<gi>publisher</gi>).</desc>
  <desc xml:lang="fr" versionDate="2023-04-05">Contient, pour les imprimés, le lieu d’impression
    (<gi>pubPlace</gi>) et le nom de l'imprimerie (<gi>publisher</gi>).</desc>
  <classes mode="replace">
    <memberOf key="model.titlepagePart"/>
  </classes>
  <content>
    <alternate>
      <elementRef key="pubPlace" maxOccurs="unbounded"/>
      <elementRef key="publisher"/>
      <sequence>
        <elementRef key="pubPlace" maxOccurs="unbounded"/>
        <elementRef key="publisher"/>
      </sequence>
    </alternate>
  </content>
