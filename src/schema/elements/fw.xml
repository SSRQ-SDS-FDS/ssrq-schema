<elementSpec xmlns="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" ident="fw" module="transcr" mode="change">
  <gloss xml:lang="de" versionDate="2023-07-13">forme work</gloss>
  <gloss xml:lang="en" versionDate="2023-07-13">forme work</gloss>
  <gloss xml:lang="fr" versionDate="2023-07-13">forme work</gloss>
  <desc xml:lang="de" versionDate="2023-08-18">Enthält Kustoden bzw. Reklamanten. Das sich am
    unteren Blattende befindliche Wort wird in der Regel weggelassen bzw. auf der nächsten Seite
    transkribiert.</desc>
  <desc xml:lang="en" versionDate="2023-08-18">Contains catchwords and reclamants. The word at the
    bottom of the page is usually omitted or transcribed on the next page.</desc>
  <desc xml:lang="fr" versionDate="2023-08-18">Contient des mots clés et des réclamations. Le mot
    qui se trouve au bas de la page est généralement omis ou transcrit sur la page suivante.</desc>
  <classes mode="replace">
    <memberOf key="att.typed"/>
  </classes>
  <content>
    <sequence maxOccurs="unbounded">
      <macroRef key="ssrq.content.default"/>
    </sequence>
  </content>
  <attList>
    <attDef ident="type" mode="replace" usage="req">
      <datatype>
        <dataRef key="ssrq.type.fw"/>
      </datatype>
    </attDef>
  </attList>
  <xi:include href="examples.xml" xpointer="ex-fw-de"/>
  <xi:include href="examples.xml" xpointer="ex-fw-en"/>
  <xi:include href="examples.xml" xpointer="ex-fw-fr"/>
