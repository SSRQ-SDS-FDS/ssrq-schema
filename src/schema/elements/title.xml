<elementSpec xmlns="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" ident="title" module="core" mode="change">
  <gloss xml:lang="de" versionDate="2025-02-04">Titel</gloss>
  <gloss xml:lang="en" versionDate="2025-02-04">title</gloss>
  <gloss xml:lang="fr" versionDate="2025-02-04">titre</gloss>
  <desc xml:lang="de" versionDate="2025-02-07">Enthält als Teil des <gi>titleStmt</gi> den Titel
    des Bandes bzw. der Editionseinheit, als Teil des <gi>seriesStmt</gi> den Titel
    der Reihe SSRQ.</desc>
  <desc xml:lang="en" versionDate="2023-07-20">Contains the title of the volume of legal sources
    or the edition unit as part of the <gi>titleStmt</gi>, and the title of the series as part of
    the <gi>seriesStmt</gi>.</desc>
  <desc xml:lang="fr" versionDate="2025-02-07">Contient le titre du volume ou de l'unité d'édition
    dans le cadre du <gi>titleStmt</gi>, et le titre de la série SDS dans le cadre de la
    <gi>seriesStmt</gi>.</desc>
  <classes mode="replace">
    <memberOf key="att.global"/>
    <memberOf key="model.emphLike"/>
    <memberOf key="model.msQuoteLike"/>
  </classes>
  <content>
    <alternate maxOccurs="unbounded">
      <textNode/>
      <elementRef key="pc"/>
      <elementRef key="hi"/>
    </alternate>
  </content>
  <attList>
    <attDef ident="level" mode="delete"/>
    <attDef ident="n" mode="delete"/>
    <attDef ident="type" mode="delete"/>
    <attDef ident="xml:id" mode="delete"/>
  </attList>
  <xi:include href="examples.xml" xpointer="ex-title-de"/>
  <xi:include href="examples.xml" xpointer="ex-title-en"/>
  <xi:include href="examples.xml" xpointer="ex-title-fr"/>
