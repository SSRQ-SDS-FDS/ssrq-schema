<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl"
    type="application/xml"
    schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" ident="custEvent" module="msdescription" mode="change">
  <desc xml:lang="de" versionDate="2023-09-19">Beschreibt ein Ereignis in der Aufbewahrungsgeschichte eines Dokuments.</desc>
  <desc xml:lang="en" versionDate="2023-09-19">Describes an event in the custodial history of a document.</desc>
  <desc xml:lang="fr" versionDate="2023-09-19">Décrit un événement dans l'historique de conservation d'un document.</desc>
  <classes mode="replace">
    <memberOf key="att.datable"/>
    <memberOf key="att.datable.custom"/>
    <memberOf key="att.typed"/>
  </classes>
  <content>
    <textNode/>
  </content>
  <attList>
    <attDef ident="calendar" mode="change">
      <constraintSpec ident="calendar-check-custEvent" mode="delete"/>
    </attDef>
    <attDef ident="type" mode="replace" usage="req">
      <desc xml:lang="de" versionDate="2023-09-19">Art des Ereignisses.</desc>
      <desc xml:lang="en" versionDate="2023-09-19">Type of event.</desc>
      <desc xml:lang="fr" versionDate="2023-09-19">Type d'événement.</desc>
      <valList type="closed">
        <valItem ident="lost">
          <desc xml:lang="de" versionDate="2023-09-19">Verlust des Dokuments.</desc>
          <desc xml:lang="en" versionDate="2023-09-19">Loss of the document.</desc>
          <desc xml:lang="fr" versionDate="2023-09-19">Perte du document.</desc>
          <desc xml:lang="it" versionDate="2023-09-19">Perdita del documento.</desc>
        </valItem>
      </valList>
    </attDef>
  </attList>
  <xi:include href="examples.xml" xpointer="ex-adminInfo-de"/>
  <xi:include href="examples.xml" xpointer="ex-adminInfo-fr"/>
  <xi:include href="examples.xml" xpointer="ex-adminInfo-en"/>
</elementSpec>
