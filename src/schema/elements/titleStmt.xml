<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl"
        type="application/xml"
        schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0" ident="titleStmt" module="header" mode="change">
  <gloss xml:lang="de" versionDate="2023-07-18">Angaben zum Titel</gloss>
  <gloss xml:lang="en" versionDate="2023-07-18">Title statement</gloss>
  <gloss xml:lang="fr" versionDate="2023-07-18">Informations sur le titre</gloss>
  <gloss xml:lang="it" versionDate="2023-07-18">Informazioni sul titolo</gloss>
  <desc xml:lang="de" versionDate="2023-07-18">Enthält Elemente zur Beschreibung des Titels und der
        Verantwortlichkeiten.
    </desc>
  <desc xml:lang="en" versionDate="2023-07-18">Contains elements describing title and responsibilities.
    </desc>
  <desc xml:lang="fr" versionDate="2023-07-18">Contient des éléments décrivant le titre et les responsabilités.
    </desc>
  <desc xml:lang="it" versionDate="2023-07-18">Contiene elementi che descrivono titolo e responsabilità.
    </desc>
  <classes mode="replace"/>
  <content>
    <sequence>
      <elementRef key="title"/>
      <elementRef key="editor" maxOccurs="unbounded"/>
      <elementRef key="respStmt" maxOccurs="unbounded"/>
    </sequence>
  </content>
  <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-04-25">
    <p>Beispielhaftes titleStmt mit zwei Editor*innen</p>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <titleStmt>
        <title>I. Abteilung: Die Rechtsquellen des Kantons Zürich. Neue Folge. Zweiter Teil:
                    Rechte der Landschaft. Band 11: Die Obervogteien um die Stadt Zürich
                </title>
        <editor>
          <persName>Ariane Huber Hernández</persName>
        </editor>
        <editor>
          <persName>Michael Nadig</persName>
        </editor>
        <respStmt>
          <persName>Ariane Huber Hernández</persName>
          <persName>Michael Nadig</persName>
          <resp>Transkription</resp>
        </respStmt>
        <respStmt>
          <persName>Ariane Huber Hernández</persName>
          <persName>Michael Nadig</persName>
          <resp>XML-Codierung der Transkription</resp>
        </respStmt>
      </titleStmt>
    </egXML>
  </exemplum>
  <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-04-25">
    <p>Exemple de titleStmt avec deux rédacteurs</p>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <titleStmt>
        <title>I. Abteilung: Die Rechtsquellen des Kantons Zürich. Neue Folge. Zweiter Teil:
                    Rechte der Landschaft. Band 11: Die Obervogteien um die Stadt Zürich
                </title>
        <editor>
          <persName>Ariane Huber Hernández</persName>
        </editor>
        <editor>
          <persName>Michael Nadig</persName>
        </editor>
        <respStmt>
          <persName>Ariane Huber Hernández</persName>
          <persName>Michael Nadig</persName>
          <resp>Transkription</resp>
        </respStmt>
        <respStmt>
          <persName>Ariane Huber Hernández</persName>
          <persName>Michael Nadig</persName>
          <resp>XML-Codierung der Transkription</resp>
        </respStmt>
      </titleStmt>
    </egXML>
  </exemplum>
</elementSpec>
