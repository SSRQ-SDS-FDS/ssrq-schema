<elementSpec xmlns="http://www.tei-c.org/ns/1.0" ident="profileDesc" module="header" mode="change">
  <desc xml:lang="de" versionDate="2023-07-18">Enthält Elemente zur Beschreibung des Textes mithilfe von
        Schlagworten.
    </desc>
  <desc xml:lang="en" versionDate="2023-07-18">Contains elements to describe the text using keywords.
    </desc>
  <desc xml:lang="fr" versionDate="2023-07-18">Contient des éléments pour décrire le texte à l'aide de mots-clés.
    </desc>
  <desc xml:lang="it" versionDate="2023-07-18">Contiene elementi per descrivere il testo utilizzando parole chiave.
    </desc>
  <classes mode="replace">
    <memberOf key="model.teiHeaderPart"/>
  </classes>
  <content>
    <elementRef key="textClass"/>
  </content>
  <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-04-25">
    <p>Modellhafter Aufbau</p>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <profileDesc>
        <textClass>
          <keywords>
            <term ref="key000192"/>
            <term ref="key000097"/>
            <term ref="key000049"/>
            <term ref="key000193"/>
          </keywords>
        </textClass>
      </profileDesc>
    </egXML>
  </exemplum>
  <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-04-25">
    <p>Structure modèle</p>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <profileDesc>
        <textClass>
          <keywords>
            <term ref="key000192"/>
            <term ref="key000097"/>
            <term ref="key000049"/>
            <term ref="key000193"/>
          </keywords>
        </textClass>
      </profileDesc>
    </egXML>
  </exemplum>
</elementSpec>
