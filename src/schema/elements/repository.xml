<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl"
        type="application/xml"
        schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" ident="repository" module="msdescription" mode="change">
  <desc xml:lang="de" versionDate="2023-07-15">Enthält ohne Abkürzung den Namen des Archivs, in welchem die Quelle
        liegt.
    </desc>
  <desc xml:lang="en" versionDate="2023-07-15">Contains the name of the archive in which the source is located without
        abbreviation.
    </desc>
  <desc xml:lang="fr" versionDate="2023-07-15">Contient le nom de l'archive dans laquelle se trouve la source sans
        abréviation.
    </desc>
  <desc xml:lang="it" versionDate="2023-07-15">Contiene il nome dell'archivio in cui si trova la fonte senza
        abbreviazione.
    </desc>
  <classes mode="replace">
    <memberOf key="att.global"/>
  </classes>
  <content>
    <textNode/>
  </content>
  <attList>
    <attDef ident="n" mode="delete"/>
    <attDef ident="xml:id" mode="delete"/>
    <attDef ident="xml:lang" mode="replace" usage="req">
      <desc xml:lang="de" versionDate="2023-06-23">ISO-639-1-Sprachkürzel</desc>
      <desc xml:lang="en" versionDate="2023-06-23">ISO-639-1-Language abbreviation</desc>
      <desc xml:lang="fr" versionDate="2023-06-23">ISO-639-1-Abréviation de la langue</desc>
      <desc xml:lang="it" versionDate="2023-06-23">ISO-639-1-Abbreviazione della lingua</desc>
      <valList type="closed">
        <valItem ident="de"/>
        <valItem ident="fr"/>
      </valList>
    </attDef>
  </attList>
  <xi:include href="examples.xml" xpointer="ex-msIdentifier-de"/>
  <xi:include href="examples.xml" xpointer="ex-msIdentifier-en"/>
  <xi:include href="examples.xml" xpointer="ex-msIdentifier-fr"/>
  <xi:include href="examples.xml" xpointer="ex-msIdentifier-it"/>
</elementSpec>
