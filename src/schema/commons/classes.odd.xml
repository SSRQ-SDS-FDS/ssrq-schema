            <rng:choice>
              <rng:ref name="ssrq.hand"/>
              <rng:ref name="ssrq.hand.century"/>
              <rng:data type="string"/>
            </rng:choice>
          </datatype>
        </attDef>
      </attList>
    </classSpec>
