        <specGrpRef target="elements/sourceDesc.intro.xml"/>
        <specGrpRef target="elements/table.intro.xml"/>
        <specGrpRef target="elements/teiHeader.intro.xml"/>
        <specGrpRef target="elements/TEI.xml"/>
        <specGrpRef target="elements/text.intro.xml"/>
