<?xml version="1.0" encoding="UTF-8"?>
<?xml-model
        href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl"
        type="application/xml"
        schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0"
             ident="msIdentifier"
             module="msdescription"
             mode="change">
    <desc xml:lang="de" versionDate="2023-07-15">Beschreibt den aktuellen und ehemalige Aufbewahrungsorte der Quelle.
    </desc>
    <desc xml:lang="en" versionDate="2023-07-15">Describes the current and historical locations of the source.
    </desc>
    <desc xml:lang="fr" versionDate="2023-07-15">Décrit les emplacements actuels et historiques de la source.
    </desc>
    <desc xml:lang="it" versionDate="2023-07-15">Descrive le posizioni attuali e storiche della fonte.
    </desc>
    <classes mode="replace"/>
    <content>
        <sequence>
            <sequence preserveOrder="false" maxOccurs="unbounded">
                <elementRef key="idno"/>
                <elementRef key="repository"/>
                <elementRef key="settlement" minOccurs="0"/>
            </sequence>
            <elementRef key="altIdentifier" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
    </content>
    <!-- This overrides an erroneous constraint in the TEI guideline -->
    <constraintSpec scheme="schematron" ident="msId_minimal" mode="delete"/>
    <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-04-13">
        <p>Beispielhafte Verwendung des Elements tei:msIdentifier</p>
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <msIdentifier>
                <repository xml:lang="de">Staatsarchiv Schwyz</repository>
                <idno xml:lang="de">StASZ, Urk. 190</idno>
            </msIdentifier>
        </egXML>
    </exemplum>
    <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-04-13">
        <p>Exemple d'utilisation de l'élément tei:msIdentifier</p>
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <msIdentifier>
                <repository xml:lang="de">Staatsarchiv Schwyz</repository>
                <idno xml:lang="de">StASZ, Urk. 190</idno>
            </msIdentifier>
        </egXML>
    </exemplum>
</elementSpec>