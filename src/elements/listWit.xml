<?xml version="1.0" encoding="UTF-8"?>
<?xml-model
        href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl"
        type="application/xml"
        schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0"
             ident="listWit"
             module="textcrit"
             mode="change">
    <gloss xml:lang="de" versionDate="2023-07-13">Textzeugenliste</gloss>
    <gloss xml:lang="en" versionDate="2023-07-13">Text witness list</gloss>
    <gloss xml:lang="fr" versionDate="2023-07-13">Liste des témoins textuelle</gloss>
    <gloss xml:lang="it" versionDate="2023-07-13">Elenco dei testimoni di testo</gloss>
    <desc xml:lang="de" versionDate="2023-07-13">Enthält die Stückbeschreibungen der Quellen.</desc>
    <desc xml:lang="en" versionDate="2023-07-13">Contains the piece descriptions of the sources.</desc>
    <desc xml:lang="fr" versionDate="2023-07-13">Contient les descriptions des pièces des sources.</desc>
    <desc xml:lang="it" versionDate="2023-07-13">Contiene le descrizioni dei pezzi delle fonti.</desc>
    <classes mode="replace">
        <memberOf key="model.listLike"/>
    </classes>
    <content>
        <sequence maxOccurs="unbounded">
            <elementRef key="witness"/>
        </sequence>
    </content>
    <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-05-02">
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <listWit>
                <witness xml:id="ad28656b-5c8d-459c-afb4-3e6ddf70810d" n="A">
                    <msDesc>
                        <msIdentifier>
                            <idno>STAW B 3a/10</idno>
                        </msIdentifier>
                    </msDesc>
                </witness>
                <witness xml:id="ad28656b-5c8d-459c-afb4-3e6ddf70810e" n="B">
                    <msDesc>
                        <msIdentifier>
                            <idno>winbib Ms. Fol. 27</idno>
                        </msIdentifier>
                    </msDesc>
                </witness>
            </listWit>
        </egXML>
    </exemplum>
    <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-05-02">
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <listWit>
                <witness xml:id="ad28656b-5c8d-459c-afb4-3e6ddf70810d" n="A">
                    <msDesc>
                        <msIdentifier>
                            <idno>STAW B 3a/10</idno>
                        </msIdentifier>
                    </msDesc>
                </witness>
                <witness xml:id="ad28656b-5c8d-459c-afb4-3e6ddf70810e" n="B">
                    <msDesc>
                        <msIdentifier>
                            <idno>winbib Ms. Fol. 27</idno>
                        </msIdentifier>
                    </msDesc>
                </witness>
            </listWit>
        </egXML>
    </exemplum>
    <remarks xml:lang="de" versionDate="2023-07-13">
        <p>Für jede Quelle wird ein <gi>witness</gi> erstellt.
        </p>
    </remarks>
    <remarks xml:lang="en" versionDate="2023-07-13">
        <p>A <gi>witness</gi> is created for each source.
        </p>
    </remarks>
    <remarks xml:lang="fr" versionDate="2023-07-13">
        <p>Un <gi>witness</gi> est créé pour chaque source.
        </p>
    </remarks>
    <remarks xml:lang="it" versionDate="2023-07-13">
        <p>Per ogni fonte viene creato un <gi>witness</gi>.
        </p>
    </remarks>
</elementSpec>