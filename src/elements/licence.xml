<?xml version="1.0" encoding="UTF-8"?>
<?xml-model
        href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl"
        type="application/xml"
        schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0"
             ident="licence"
             module="header"
             mode="change">
    <desc xml:lang="de" versionDate="2023-07-18">Beschreibt die Lizenz, unter der ein Text verfügbar ist.</desc>
    <desc xml:lang="en" versionDate="2023-07-18">Describes the license under which a text is available.</desc>
    <desc xml:lang="fr" versionDate="2023-07-18">Décrit la licence sous laquelle un texte est disponible.</desc>
    <desc xml:lang="it" versionDate="2023-07-18">Descrive la licenza in base alla quale un testo è disponibile.</desc>
    <classes mode="replace">
        <memberOf key="att.pointing"/>
        <memberOf key="model.availabilityPart"/>
    </classes>
    <content>
        <valList type="closed">
            <valItem ident="Attribution-NonCommercial-ShareAlike 4.0 International (CC BY-NC-SA 4.0)"/>
        </valList>
    </content>
    <attList>
        <attDef ident="target" mode="replace" usage="req">
            <valList type="closed">
                <valItem ident="https://creativecommons.org/licenses/by-nc-sa/4.0/"/>
            </valList>
        </attDef>
    </attList>
    <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-03-30">
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <licence target="https://creativecommons.org/licenses/by-nc-sa/4.0/">
                Attribution-NonCommercial-ShareAlike 4.0 International (CC BY-NC-SA 4.0)
            </licence>
        </egXML>
    </exemplum>
    <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-03-30">
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <licence target="https://creativecommons.org/licenses/by-nc-sa/4.0/">
                Attribution-NonCommercial-ShareAlike 4.0 International (CC BY-NC-SA 4.0)
            </licence>
        </egXML>
    </exemplum>
</elementSpec>