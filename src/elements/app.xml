                <rdg wit="ad28656b-5c8d-459c-afb4-3e6ddf70810d ad28656b-5c8d-459c-afb4-3e6ddf70810e">
                    quadringentesimo
                </rdg>
            </app>
        </egXML>
    </exemplum>
    <exemplum type="ssrq-doc-example" versionDate="2023-05-17">
