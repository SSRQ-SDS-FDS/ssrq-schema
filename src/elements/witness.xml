<elementSpec xmlns="http://www.tei-c.org/ns/1.0" ident="witness" module="textcrit" mode="change">
    <desc xml:lang="de" versionDate="2023-04-26">
        <gi>witness</gi>
        enthält die Stückbeschreibung einer Quelle.
    </desc>
    <desc xml:lang="en" versionDate="2023-04-26">
        <gi>witness</gi>
        ccontains the description of a source.
    </desc>
