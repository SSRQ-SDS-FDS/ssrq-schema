<?xml version="1.0" encoding="UTF-8"?>
<?xml-model
        href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl"
        type="application/xml"
        schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0"
             xmlns:rng="http://relaxng.org/ns/structure/1.0"
             ident="width"
             module="msdescription"
             mode="change">
    <desc xml:lang="de" versionDate="2023-07-15">Beschreibt die Breite eines Blattes oder Buchs sowie die Breite der
        Plica.
    </desc>
    <desc xml:lang="en" versionDate="2023-07-15">Describes the width of a sheet or book and the width of the plica.
    </desc>
    <desc xml:lang="fr" versionDate="2023-07-15">Décrit la largeur d'une feuille ou d'un livre et la largeur du plica.
    </desc>
    <desc xml:lang="it" versionDate="2023-07-15">Descrive la larghezza di un foglio o di un libro e la larghezza della
        plica.
    </desc>
    <classes mode="replace">
        <memberOf key="att.dimensions"/>
        <memberOf key="model.dimLike"/>
        <memberOf key="model.measureLike"/>
    </classes>
    <content>
        <empty/>
    </content>
    <attList>
        <attDef ident="unit" mode="change" usage="req">
            <datatype>
                <rng:choice>
                    <rng:ref name="ssrq.measure.unknown"/>
                    <rng:ref name="ssrq.measure.lengths.base.cm"/>
                </rng:choice>
            </datatype>
        </attDef>
        <attDef ident="quantity" mode="change" usage="req"/>
    </attList>
    <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-03-17">
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <width unit="cm" quantity="23.0"/>
        </egXML>
    </exemplum>
    <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-03-17">
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <width unit="cm" quantity="23.0"/>
        </egXML>
    </exemplum>
    <remarks xml:lang="de" versionDate="2023-07-14">
        <p>Die Masseinheit für <att>unit</att> ist Zentimeter (cm).
        </p>
    </remarks>
    <remarks xml:lang="en" versionDate="2023-07-14">
        <p>The unit of measure for <att>unit</att> is centimeters (cm).
        </p>
    </remarks>
    <remarks xml:lang="fr" versionDate="2023-07-14">
        <p>L'unité de mesure pour <att>unit</att> est le centimètre (cm).
        </p>
    </remarks>
    <remarks xml:lang="it" versionDate="2023-07-14">
        <p>L'unità di misura per <att>unit</att> è il centimetro (cm).
        </p>
    </remarks>
</elementSpec>