<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl" type="application/xml" schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>

<elementSpec xmlns="http://www.tei-c.org/ns/1.0" ident="space" module="transcr" mode="change">
    <desc xml:lang="de" versionDate="2023-05-10">zeichnet eine Lücke aus, die vom Schreiber bewusst zwecks späterer
        Ergänzung offen gelassen wurde.
    </desc>
    <desc xml:lang="en" versionDate="2023-05-10">denotes a gap that the writer deliberately left open for later
        completion.
    </desc>
    <desc xml:lang="fr" versionDate="2023-05-10">désigne une lacune que l'auteur a délibérément laissée ouverte pour une
        réalisation ultérieure.
    </desc>
    <classes mode="replace">
        <memberOf key="att.dimensions"/>
    </classes>
    <content>
        <empty/>
    </content>
    <attList>
        <attDef ident="dim" mode="delete"/>
        <attDef ident="extent" mode="delete"/>
        <attDef ident="unit" mode="change" usage="req"/>
        <attDef ident="quantity" mode="change" usage="req"/>
    </attList>
    <exemplum type="ssrq-doc-example" versionDate="2023-05-03">
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <space unit="word" quantity="2"/>
        </egXML>
    </exemplum>
</elementSpec>