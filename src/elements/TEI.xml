<?xml version="1.0" encoding="UTF-8"?>
<?xml-model
        href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl"
        type="application/xml"
        schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0"
             ident="TEI"
             module="textstructure"
             mode="change">
    <gloss xml:lang="de" versionDate="2023-03-09">TEI Dokument</gloss>
    <gloss xml:lang="en" versionDate="2023-03-09">TEI Dokument</gloss>
    <gloss xml:lang="fr" versionDate="2023-03-09">Document TEI</gloss>
    <gloss xml:lang="it" versionDate="2023-03-09">documento TEI</gloss>
    <desc xml:lang="de" versionDate="2023-07-14">Beinhaltet ein einzelnes Dokument (Stück) als sogenanntes
        Root-Element.
    </desc>
    <desc xml:lang="en" versionDate="2023-07-14">Contains a single document (piece) as a so-called root element.
    </desc>
    <desc xml:lang="fr" versionDate="2023-07-14">Contient un seul document (morceau) en tant qu'élément dit root.
    </desc>
    <desc xml:lang="it" versionDate="2023-07-14">Contiene un singolo documento (pezzo) come cosiddetto elemento root.
    </desc>
    <classes mode="replace">
        <memberOf key="att.global"/>
    </classes>
    <content>
        <sequence>
            <elementRef key="teiHeader"/>
            <elementRef key="text"/>
        </sequence>
    </content>
    <attList>
        <attDef ident="n" mode="delete"/>
        <attDef ident="xml:id" mode="delete"/>
        <attDef ident="xml:lang" mode="replace">
            <valList type="closed">
                <valItem ident="de"/>
                <valItem ident="fr"/>
                <valItem ident="it"/>
            </valList>
        </attDef>
        <attDef ident="version" mode="delete"/>
    </attList>
    <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-03-03">
        <p>Minimales Beispiel:</p>
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <TEI xml:lang="de">
                <teiHeader/>
                <text/>
            </TEI>
        </egXML>
    </exemplum>
    <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-03-03">
        <p>Exemple minimal :</p>
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <TEI xml:lang="de">
                <teiHeader/>
                <text/>
            </TEI>
        </egXML>
    </exemplum>
    <exemplum xml:lang="en" type="ssrq-doc-example" versionDate="2023-03-03">
        <p>Minimal example:</p>
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <TEI xml:lang="de">
                <teiHeader/>
                <text/>
            </TEI>
        </egXML>
    </exemplum>
    <remarks xml:lang="de" versionDate="2023-07-14">
        <p>Mit <att>xml:lang</att> wird die Sprache der Bearbeitung angegeben.
        </p>
    </remarks>
    <remarks xml:lang="en" versionDate="2023-07-14">
        <p>The language of the processing is specified with <att>xml:lang</att>.
        </p>
    </remarks>
    <remarks xml:lang="fr" versionDate="2023-07-14">
        <p>La langue du traitement est spécifiée avec <att>xml:lang</att>.
        </p>
    </remarks>
    <remarks xml:lang="it" versionDate="2023-07-14">
        <p><att>xml:lang</att>specifica la lingua di editing.
        </p>
    </remarks>
</elementSpec>