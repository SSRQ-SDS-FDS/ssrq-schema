<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl" type="application/xml" schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0" ident="TEI" module="textstructure" mode="change">
    <gloss xml:lang="de" versionDate="2023-03-09">TEI Dokument</gloss>
    <desc xml:lang="de" versionDate="2023-05-11">Umschliesst ein einzelnes Dokument
        (Stück) als sogenannter Root-Tag. Mehrfachüberlieferungen werden in einem Tag teiCorpus gebündelt.
    </desc>
    <gloss xml:lang="en" versionDate="2023-03-09">TEI Dokument</gloss>
    <desc xml:lang="en" versionDate="2023-05-11">Encloses a single document (piece) as a
        so-called root tag. Multiple deliveries are bundled in a teiCorpus tag.
    </desc>
    <gloss xml:lang="fr" versionDate="2023-03-09">Document TEI</gloss>
    <desc xml:lang="fr" versionDate="2023-05-11">Entoure un document individuel
        (pièce). Les documents à transmission multiple sont regroupés dans une balise teiCorpus.
    </desc>
    <classes mode="change">
        <memberOf key="att.global.facs" mode="delete"/>
        <memberOf key="att.global.rendition" mode="delete"/>
        <memberOf key="att.global.responsibility" mode="delete"/>
        <memberOf key="att.typed" mode="delete"/>
    </classes>
    <attList>
        <attDef ident="xml:lang" mode="replace">
            <valList type="closed">
                <valItem ident="de"/>
                <valItem ident="it"/>
                <valItem ident="fr"/>
            </valList>
        </attDef>
        <attDef ident="n" mode="delete"/>
        <attDef ident="xml:id" mode="delete"/>
    </attList>
    <exemplum type="ssrq-doc-example" versionDate="2023-03-03">
        <p xml:lang="de">Minimales Beispiel:</p>
        <p xml:lang="fr">Exemple minimal&#160;:</p>
        <p xml:lang="en">Minimal example:</p>
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <TEI xml:lang="de">
                <teiHeader></teiHeader>
                <text></text>
            </TEI>
        </egXML>
    </exemplum>
</elementSpec>
