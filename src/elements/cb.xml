<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl" type="application/xml" schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0"
             ident="cb"
             module="core"
             mode="change">
   <gloss xml:lang="de" versionDate="2023-05-23">Spaltenbeginn</gloss>
   <gloss xml:lang="en" versionDate="2023-05-23">Column start</gloss>
   <gloss xml:lang="fr" versionDate="2023-05-23">Début de colonne</gloss>
   <gloss xml:lang="it" versionDate="2023-05-23">Inizio colonna</gloss>
   <desc xml:lang="de" versionDate="2023-05-23">Beschreibt den Beginn einer neuen Spalte.</desc>
   <desc xml:lang="en" versionDate="2023-05-23">Describes the beginning of a new column.</desc>
   <desc xml:lang="fr" versionDate="2023-05-23">Décrit le début d'une nouvelle colonne.</desc>
   <desc xml:lang="it" versionDate="2023-05-23">Descrive l'inizio di una nuova colonna.</desc>
   <classes mode="replace">
      <memberOf key="model.milestoneLike"/>
      <memberOf key="att.gobal"/>
   </classes>
   <attList>
      <attDef ident="n" mode="replace">
         <desc xml:lang="de" versionDate="2023-05-23">Enthält Angaben zur originalen Spaltenzählung.</desc>
         <desc xml:lang="en" versionDate="2023-05-23">Contains information about the original column count.</desc>
         <desc xml:lang="fr" versionDate="2023-05-23">Contient des informations sur le nombre de colonnes
                d'origine.
            </desc>
         <desc xml:lang="it" versionDate="2023-05-23">Contiene informazioni sul conteggio delle colonne originali.
            </desc>
         <datatype>
            <dataRef name="string" restriction="[a-z]{1}"/>
         </datatype>
      </attDef>
      <attDef ident="xml:id" mode="delete"/>
      <attDef ident="xml:lang" mode="delete"/>
      <attDef ident="cert" mode="delete"/>
      <attDef ident="rend" mode="delete"/>
      <attDef ident="resp" mode="delete"/>
   </attList>
   <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-05-23">
      <p>Beispiel für den Beginn einer neuen Spalte.</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <pb n="1r"/>
         <cb n="a"/>
         <p>...</p>
         <p>...</p>
         <cb n="b"/>
         <p>...</p>
         <p>...</p>
         <pb n="1v"/>
      </egXML>
   </exemplum>
   <exemplum xml:lang="en" type="ssrq-doc-example" versionDate="2023-05-23">
      <p>Example of starting a new column.</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <pb n="1r"/>
         <cb n="a"/>
         <p>...</p>
         <p>...</p>
         <cb n="b"/>
         <p>...</p>
         <p>...</p>
         <pb n="1v"/>
      </egXML>
   </exemplum>
   <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-05-23">
      <p>Exemple de démarrage d'une nouvelle colonne.</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <pb n="1r"/>
         <cb n="a"/>
         <p>...</p>
         <p>...</p>
         <cb n="b"/>
         <p>...</p>
         <p>...</p>
         <pb n="1v"/>
      </egXML>
   </exemplum>
   <exemplum xml:lang="it" type="ssrq-doc-example" versionDate="2023-05-23">
      <p>Esempio di inizio di una nuova colonna.</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <pb n="1r"/>
         <cb n="a"/>
         <p>...</p>
         <p>...</p>
         <cb n="b"/>
         <p>...</p>
         <p>...</p>
         <pb n="1v"/>
      </egXML>
   </exemplum>
</elementSpec>
