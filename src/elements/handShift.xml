<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl" type="application/xml" schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0"
             ident="handShift"
             module="transcr"
             mode="change">
   <gloss xml:lang="de" versionDate="2023-03-15">Handwechsel</gloss>
   <desc xml:lang="de" versionDate="2023-05-11">Gibt den Wechsel einer Hand an.</desc>
   <gloss xml:lang="en" versionDate="2023-03-15">Change of hands</gloss>
   <desc xml:lang="en" versionDate="2023-05-11">Indicates the change of a hand.</desc>
   <gloss xml:lang="fr" versionDate="2023-03-15">Changement de main</gloss>
   <desc xml:lang="fr" versionDate="2023-05-11">Indique le changement d'une main.</desc>
   <gloss xml:lang="it" versionDate="2023-05-20">Cambio di mano</gloss>
   <desc xml:lang="it" versionDate="2023-05-20">Indica il cambio di mano.</desc>
   <classes mode="replace">
      <memberOf key="att.written"/>
   </classes>
   <content>
      <empty/>
   </content>
   <attList>
      <attDef ident="hand" mode="change" usage="req"/>
      <attDef ident="new" mode="delete"/>
   </attList>
   <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-05-20">
      <p>Wechsel zu einer anderen Hand</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <handShift hand="otherHand"/>
      </egXML>
   </exemplum>
   <exemplum xml:lang="en" type="ssrq-doc-example" versionDate="2023-05-20">
      <p>switch to another hand</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <handShift hand="otherHand"/>
      </egXML>
   </exemplum>
   <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-05-20">
      <p>passer à une autre main</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <handShift hand="otherHand"/>
      </egXML>
   </exemplum>
   <exemplum xml:lang="it" type="ssrq-doc-example" versionDate="2023-05-20">
      <p>passa a un'altra mano</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <handShift hand="otherHand"/>
      </egXML>
   </exemplum>
   <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-05-20">
      <p>Wechsel zu einer zweiten Hand</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <handShift hand="secondHand"/>
      </egXML>
   </exemplum>
   <exemplum xml:lang="en" type="ssrq-doc-example" versionDate="2023-05-20">
      <p>switch to a second hand</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <handShift hand="secondHand"/>
      </egXML>
   </exemplum>
   <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-05-20">
      <p>Passer à une seconde main</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <handShift hand="secondHand"/>
      </egXML>
   </exemplum>
   <exemplum xml:lang="it" type="ssrq-doc-example" versionDate="2023-05-20">
      <p>Passa a una mano seconda</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <handShift hand="secondHand"/>
      </egXML>
   </exemplum>
</elementSpec>
