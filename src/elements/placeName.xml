<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl"
        type="application/xml"
        schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" ident="placeName" module="namesdates" mode="change">
  <desc xml:lang="de" versionDate="2023-04-20">Zeichnet topographische Bezeichnungen (einen Ort oder einen Flurnamen)
        aus.
    </desc>
  <desc xml:lang="en" versionDate="2023-04-20">Indicates topographical designations (a place or a field name).</desc>
  <desc xml:lang="fr" versionDate="2023-04-20">Distingue les désignations topographiques (un lieu ou un lieu-dit).
    </desc>
  <desc xml:lang="it" versionDate="2023-04-20">Contrassegna designazioni topografiche (un luogo o un nome di campo).
    </desc>
  <classes mode="replace">
    <memberOf key="att.canonical"/>
    <memberOf key="model.placeNamePart"/>
  </classes>
  <content>
    <rng:oneOrMore>
      <rng:ref name="ssrq.content.default"/>
    </rng:oneOrMore>
  </content>
  <attList>
    <attDef ident="ref" mode="replace">
      <desc xml:lang="de" versionDate="2023-03-28">Verknüpfung des Ortes mit der ID der
                Ortsdatenbank.
            </desc>
      <desc xml:lang="en" versionDate="2023-03-28">Linking the place with the ID of the database
                of places.
            </desc>
      <desc xml:lang="fr" versionDate="2023-03-28">Relier le lieu à l'ID de la base de données
                des lieux.
            </desc>
      <desc xml:lang="it" versionDate="2023-03-28">Associazione del luogo con l'ID del database dei luoghi.
            </desc>
      <datatype>
        <dataRef key="ssrq.pointer.places"/>
      </datatype>
    </attDef>
  </attList>
  <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-07-11">
    <p>Erfassung eines (historischen) Ortsnamens (mit Verweis auf die SSRQ-Datenbank)</p>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <placeName ref="loc000284">Freiburg</placeName>
    </egXML>
  </exemplum>
  <exemplum xml:lang="en" type="ssrq-doc-example" versionDate="2023-07-11">
    <p>Recording of a (historical) place name (with reference to the SSRQ database)</p>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <placeName ref="loc000284">Fribourg</placeName>
    </egXML>
  </exemplum>
  <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-07-11">
    <p>Saisie d'un nom de lieu (historique) (avec référence à la base de données SSRQ)</p>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <placeName ref="loc000284">Frybourg</placeName>
    </egXML>
  </exemplum>
  <exemplum xml:lang="it" type="ssrq-doc-example" versionDate="2023-07-11">
    <p>Registrazione di un toponimo (storico) (con riferimento al database SSRQ)</p>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <placeName ref="loc000284">Friburgo</placeName>
    </egXML>
  </exemplum>
</elementSpec>
