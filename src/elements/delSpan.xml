<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl" type="application/xml" schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>

<elementSpec xmlns="http://www.tei-c.org/ns/1.0" ident="delSpan" module="transcr" mode="change">
    <desc xml:lang="de" versionDate="2023-05-04">bezeichnet größere Abschnitte, die aus dem Text getilgt wurden.</desc>
    <desc xml:lang="en" versionDate="2023-05-04">denotes larger sections deleted from the text.</desc>
    <desc xml:lang="fr" versionDate="2023-05-04">désigne des sections plus grandes qui ont été supprimées du texte.
    </desc>
    <classes mode="replace">
        <memberOf key="att.global.rendition"/>
        <memberOf key="att.spanning"/>
        <memberOf key="att.written"/>
    </classes>
    <content>
        <empty/>
    </content>
    <attList>
        <attDef ident="spanTo" mode="change" usage="req"/>
        <attDef ident="hand" mode="change">
            <datatype minOccurs="1" maxOccurs="1">
                <dataRef key="ssrq.hand.hand"/>
                <!-- ToDo: das hier müsste korrigiert werden, vgl. <gi>addSpan</gi>-->
            </datatype>
        </attDef>
    </attList>
    <exemplum type="ssrq-doc-example" versionDate="2023-05-03">
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <delSpan rend="crossout" hand="later hand" spanTo="#del1"/>
            Item ein hoffstatt ...
            <anchor xml:id="del1"/>
        </egXML>
    </exemplum>
</elementSpec>