<?xml version="1.0" encoding="UTF-8"?>
<?xml-model
        href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl"
        type="application/xml"
        schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0"
             ident="seriesStmt"
             module="header"
             mode="change">
    <gloss xml:lang="de" versionDate="2023-07-18">Angaben zur Reihe</gloss>
    <gloss xml:lang="en" versionDate="2023-07-18">Series statement</gloss>
    <gloss xml:lang="fr" versionDate="2023-07-18">Informations sur la série</gloss>
    <gloss xml:lang="it" versionDate="2023-07-18">Informazioni sulla serieru</gloss>
    <desc xml:lang="de" versionDate="2023-04-21">Beinhaltet Elemente zur Beschreibung der Reihe
        Sammlung Schweizerischer Rechtsquellen.
    </desc>
    <desc xml:lang="en" versionDate="2023-04-21">Contains elements for describing the collection of Swiss legal sources
        series.
    </desc>
    <desc xml:lang="fr" versionDate="2023-04-21">Contient des éléments de description de la collection de la série des
        sources juridiques suisses.
    </desc>
    <desc xml:lang="it" versionDate="2023-04-21">Contiene elementi per descrivere la collezione delle serie di fonti
        giuridiche svizzere.
    </desc>
    <classes mode="replace">
        <memberOf key="att.global"/>
    </classes>
    <content>
        <sequence>
            <elementRef key="title"/>
            <elementRef key="respStmt"/>
            <elementRef key="idno" minOccurs="2" maxOccurs="2"/>
        </sequence>
    </content>
    <attList>
        <attDef ident="n" mode="delete"/>
        <attDef ident="xml:id" mode="replace" usage="req">
            <valList type="closed">
                <valItem ident="ssrq-sds-fds"/>
            </valList>
        </attDef>
        <attDef ident="xml:lang" mode="delete"/>
    </attList>
    <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-04-21">
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <seriesStmt xml:id="ssrq-sds-fds">
                <title>Sammlung Schweizerischer Rechtsquellen</title>
                <respStmt>
                    <orgName>Rechtsquellenstiftung des Schweizerischen Juristenvereins</orgName>
                    <resp key="publisher"/>
                </respStmt>
                <idno>SSRQ-FR-I_2_8-1-1</idno>
                <idno type="uuid">ad28656b-5c8d-459c-afb4-3e6ddf70810d</idno>
            </seriesStmt>
        </egXML>
    </exemplum>
    <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-04-21">
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <seriesStmt xml:id="ssrq-sds-fds">
                <title>Sammlung Schweizerischer Rechtsquellen</title>
                <respStmt>
                    <orgName>Rechtsquellenstiftung des Schweizerischen Juristenvereins</orgName>
                    <resp key="publisher"/>
                </respStmt>
                <idno>SSRQ-FR-I_2_8-1-1</idno>
                <idno type="uuid">ad28656b-5c8d-459c-afb4-3e6ddf70810d</idno>
            </seriesStmt>
        </egXML>
    </exemplum>
</elementSpec>