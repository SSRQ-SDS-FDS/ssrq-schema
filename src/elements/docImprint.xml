<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl" type="application/xml" schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0"
             ident="docImprint"
             module="textstructure"
             mode="change">
   <desc xml:lang="de" versionDate="2023-04-05">Enthält bei Druckschriften den Druckort (pubPlace) und den Namen der
        Druckerei (publisher).
    </desc>
   <desc xml:lang="fr" versionDate="2023-04-05">Contient, pour les imprimés, le lieu d'impression (pubPlace) et le nom
        de l'imprimerie (publisher).
    </desc>
   <desc xml:lang="en" versionDate="2023-04-05">Contains the place of printing (pubPlace) and the name of the printer
        (publisher).
    </desc>
   <classes mode="replace">
      <memberOf key="model.titlepagePart"/>
   </classes>
   <content>
      <alternate>
            <elementRef key="pubPlace" minOccurs="1" maxOccurs="unbounded"/>
            <elementRef key="publisher" minOccurs="1" maxOccurs="1"/>
            <sequence>
                <elementRef key="pubPlace" minOccurs="1" maxOccurs="unbounded"/>
                <elementRef key="publisher" minOccurs="1" maxOccurs="1"/>
            </sequence>
      </alternate>
   </content>
   <exemplum xml:lang="" type="ssrq-doc-example" versionDate="2023-04-05">
      <p>Einfaches Beispiel für den Inhalt von tei:docImprint:</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <msItem>
            <textLang>Latein</textLang>
            <docImprint>
               <pubPlace>Druckort</pubPlace>
               <publisher>Druckerei</publisher>
            </docImprint>
         </msItem>
      </egXML>
   </exemplum>
</elementSpec>
