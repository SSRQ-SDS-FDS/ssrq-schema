<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl" type="application/xml" schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0"
             ident="choice"
             module="core"
             mode="change">
   <desc xml:lang="de" versionDate="2023-05-23">Gruppiert verschiedene Auszeichnungen zur selben Stelle im Text.</desc>
   <desc xml:lang="en" versionDate="2023-05-23">Groups different markup at the same place in the text.</desc>
   <desc xml:lang="fr" versionDate="2023-05-23">Regroupe différents balisages au même endroit dans le texte.</desc>
   <desc xml:lang="it" versionDate="2023-05-23">Raggruppa marcature diverse nello stesso punto del testo.</desc>
   <classes mode="replace">
      <memberOf key="model.linePart"/>
      <memberOf key="model.pPart.editorial"/>
   </classes>
   <content>
      <alternate minOccurs="1" maxOccurs="1">
         <sequence minOccurs="1" maxOccurs="1">
            <elementRef key="abbr"/>
            <elementRef key="expan"/>
         </sequence>
         <sequence minOccurs="1" maxOccurs="1">
            <elementRef key="sic"/>
            <elementRef key="corr"/>
         </sequence>
      </alternate>
   </content>
   <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-05-23">
      <p>Beispiel für eine Korrektur.</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <choice>
            <sic>offensiche</sic>
            <corr resp="anh">offenliche</corr>
         </choice>
      </egXML>
   </exemplum>
   <exemplum xml:lang="en" type="ssrq-doc-example" versionDate="2023-05-23">
      <p>Example of a correction.</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <choice>
            <sic>offensiche</sic>
            <corr resp="anh">offenliche</corr>
         </choice>
      </egXML>
   </exemplum>
   <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-05-23">
      <p>Exemple de correction.</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <choice>
            <sic>offensiche</sic>
            <corr resp="anh">offenliche</corr>
         </choice>
      </egXML>
   </exemplum>
   <exemplum xml:lang="it" type="ssrq-doc-example" versionDate="2023-05-23">
      <p>Esempio di correzione.</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <choice>
            <sic>offensiche</sic>
            <corr resp="anh">offenliche</corr>
         </choice>
      </egXML>
   </exemplum>
   <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-05-23">
      <p>Beispiel für eine Abkürzung.</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <choice>
            <abbr>sabbo</abbr>
            <expan>sabbato</expan>
         </choice>
      </egXML>
   </exemplum>
   <exemplum xml:lang="en" type="ssrq-doc-example" versionDate="2023-05-23">
      <p>Example of an abbreviation.</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <choice>
            <abbr>sabbo</abbr>
            <expan>sabbato</expan>
         </choice>
      </egXML>
   </exemplum>
   <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-05-23">
      <p>Exemple d'abréviation.</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <choice>
            <abbr>sabbo</abbr>
            <expan>sabbato</expan>
         </choice>
      </egXML>
   </exemplum>
   <exemplum xml:lang="it" type="ssrq-doc-example" versionDate="2023-05-23">
      <p>Esempio di abbreviazione.</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <choice>
            <abbr>sabbo</abbr>
            <expan>sabbato</expan>
         </choice>
      </egXML>
   </exemplum>
</elementSpec>
