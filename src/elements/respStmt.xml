<?xml version="1.0" encoding="UTF-8"?>
<?xml-model
        href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl"
        type="application/xml"
        schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0"
             ident="respStmt"
             module="core"
             mode="change">
    <gloss xml:lang="de" versionDate="2023-07-20">Angaben zur Verantwortlichkeit</gloss>
    <gloss xml:lang="en" versionDate="2023-07-20">Responsibility statement</gloss>
    <gloss xml:lang="fr" versionDate="2023-07-20">Informations sur la responsabilité</gloss>
    <gloss xml:lang="it" versionDate="2023-07-20">Informazioni sulla responsabilità</gloss>
    <desc xml:lang="de" versionDate="2023-07-20">Enthält Elemente zur Beschreibung der Verantwortlichkeit an der
        Edition.
    </desc>
    <desc xml:lang="en" versionDate="2023-07-20">Contains elements for describing the responsibility for the edition.
    </desc>
    <desc xml:lang="fr" versionDate="2023-07-20">Contient des éléments pour décrire la responsabilité de l'édition.
    </desc>
    <desc xml:lang="it" versionDate="2023-07-20">Contiene elementi per descrivere la responsabilità dell'edizione.
    </desc>
    <classes mode="replace">
        <memberOf key="model.recordingPart"/>
        <memberOf key="model.respLike"/>
    </classes>
    <content>
        <sequence>
            <alternate>
                <elementRef key="persName" maxOccurs="unbounded"/>
                <elementRef key="orgName"/>
            </alternate>
            <elementRef key="resp"/>
        </sequence>
    </content>
    <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-04-04">
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <respStmt>
                <persName>Rainer Hugener</persName>
                <resp key="transcript"/>
            </respStmt>
            <respStmt>
                <persName>Rainer Hugener</persName>
                <resp key="tagging"/>
            </respStmt>
            <respStmt>
                <persName>Werner Meier</persName>
                <resp key="facs"/>
            </respStmt>
            <respStmt>
                <persName>Pascale Sutter</persName>
                <resp key="qualitycheck"/>
            </respStmt>
        </egXML>
    </exemplum>
    <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-04-04">
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <respStmt>
                <persName>Rainer Hugener</persName>
                <resp key="transcript"/>
            </respStmt>
            <respStmt>
                <persName>Rainer Hugener</persName>
                <resp key="tagging"/>
            </respStmt>
            <respStmt>
                <persName>Werner Meier</persName>
                <resp key="facs"/>
            </respStmt>
            <respStmt>
                <persName>Pascale Sutter</persName>
                <resp key="qualitycheck"/>
            </respStmt>
        </egXML>
    </exemplum>
    <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-04-04">
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <respStmt>
                <orgName>Rechtsquellenstiftung des Schweizerischen Juristenvereins</orgName>
                <resp key="publisher"/>
            </respStmt>
        </egXML>
    </exemplum>
    <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-04-04">
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <respStmt>
                <orgName>Rechtsquellenstiftung des Schweizerischen Juristenvereins</orgName>
                <resp key="publisher"/>
            </respStmt>
        </egXML>
    </exemplum>
</elementSpec>