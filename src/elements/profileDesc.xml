<?xml-model href="http://jenkins.tei-c.org/job/TEIP5-dev/lastSuccessfulBuild/artifact/P5/release/xml/tei/odd/p5.nvdl" type="application/xml" schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0"
             ident="profileDesc"
             module="header"
             mode="change">
   <desc xml:lang="de" versionDate="2023-04-25">Enthält Informationen zur Näheren Beschreibung des
        Textes mittels einer Liste von Schlagworten.</desc>
   <desc xml:lang="en" versionDate="2023-04-25">Contains information to describe the text in more
        detail by means of a list of keywords.</desc>
   <desc xml:lang="fr" versionDate="2023-04-25">Contient des informations permettant de décrire le
        texte plus en détail au moyen d'une liste de mots-clés.</desc>
   <classes mode="replace">
      <memberOf key="model.teiHeaderPart"/>
   </classes>
   <content>
      <elementRef key="textClass"/>
   </content>
   <exemplum xml:lang="de" type="ssrq-doc-example" versionDate="2023-04-25">
      <p>Modellhafter Aufbau</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <profileDesc>
            <textClass>
               <keywords>
                  <term ref="key000192"/>
                  <term ref="key000097"/>
                  <term ref="key000049"/>
                  <term ref="key000193"/>
               </keywords>
            </textClass>
         </profileDesc>
      </egXML>
   </exemplum>
   <exemplum xml:lang="fr" type="ssrq-doc-example" versionDate="2023-04-25">
      <p>Structure modèle</p>
      <egXML xmlns="http://www.tei-c.org/ns/Examples">
         <profileDesc>
            <textClass>
               <keywords>
                  <term ref="key000192"/>
                  <term ref="key000097"/>
                  <term ref="key000049"/>
                  <term ref="key000193"/>
               </keywords>
            </textClass>
         </profileDesc>
      </egXML>
   </exemplum>
</elementSpec>
